 retrieve="SELECT	~"QTEMP~".~"BOMTEMP~".~"TCALCULATE~",  
			~"QTEMP~".~"BOMTEMP~".~"TCITN~",  
			~"QTEMP~".~"BOMTEMP~".~"TWKCT~",  
			TRANSLATE(A.ITNM,' ','~"') AS ITEMNAME, TRANSLATE(A.SPEC,' ','~"') AS SPEC,
			B.PULS AS LOTSIZE,B.PULT AS LEADTIME,
			A.XUNIT,B.XUNIT as XUNIT1 ,SUM(~"QTEMP~".~"BOMTEMP~".~"TQTY1~") AS SUMQTY,
			B.CLS,B.SRCE,B.CONVQTY,B.COSTLS,B.COSTAV,B.ABCCD,
			Case	When 	B.SRCE NOT IN ( '05','03',' ' ) THEN
							Case	When 	B.CONVQTY <> 0 Then decimal(truncate(B.COSTLS / B.CONVQTY,2),8,2)  
									Else 	B.COSTLS End 
					Else 	0.00	End AS INPUTDANGA,
			Case	When 	B.SRCE NOT IN ( '05','03',' ' ) THEN
						Case 	When 	B.CONVQTY <> 0 Then decimal(truncate(CASE	WHEN	B.COSTAV	=	0	THEN	B.COSTLS ELSE B.COSTAV END / B.CONVQTY,2),8,2)   
								Else 	CASE	WHEN	B.COSTAV	=	0	THEN	B.COSTLS ELSE B.COSTAV END End	
					Else	0.00	End AS MOVEDANGA,
			Case	When 	B.SRCE NOT IN ( '05','03',' ' ) THEN
					Case	When B.OUTQTY <> 0 Then
								Case 	When 	B.CONVQTY <> 0 Then abs(decimal(truncate(decimal(truncate(( B.OUTAMT / B.OUTQTY),2),10,2) / B.CONVQTY,2),8,2))   
										Else 	abs(decimal(truncate(( B.OUTAMT / B.OUTQTY ),2),10,2)) End  
							Else 	Case	When 	B.CONVQTY <> 0 Then decimal(truncate(B.COSTLS / B.CONVQTY,2),8,2)  
									Else 	B.COSTLS End 	END 
					Else	0.00	End AS OUTDANGA,
			~"QTEMP~".~"BOMTEMP~".~"TOPCD~",~"QTEMP~".~"BOMTEMP~".~"TOPTION~",~"QTEMP~".~"BOMTEMP~".~"TEDTM~",~"QTEMP~".~"BOMTEMP~".~"TEDTE~"
    FROM	~"QTEMP~".~"BOMTEMP~",~"PBINV~".~"INV002~" A	,	~"PBINV~".~"INV101~" B 
   WHERE B.COMLTD = '01' AND	B.COMLTD = A.COMLTD  AND	TCITN = A.ITNO  AND	TCITN = B.ITNO AND	TOLDPLANT = B.XPLANT AND	TOLDDIV = B.DIV 
	GROUP	BY		
			~"QTEMP~".~"BOMTEMP~".~"TCALCULATE~",  
			~"QTEMP~".~"BOMTEMP~".~"TCITN~",  
			~"QTEMP~".~"BOMTEMP~".~"TWKCT~",  
			A.ITNM,A.SPEC,B.PULS,B.PULT,
			A.XUNIT,B.XUNIT,
			B.CLS,B.SRCE,B.CONVQTY,B.COSTLS,B.COSTAV,B.ABCCD,B.OUTAMT,B.OUTQTY,
			~"QTEMP~".~"BOMTEMP~".~"TOPCD~",~"QTEMP~".~"BOMTEMP~".~"TOPTION~",~"QTEMP~".~"BOMTEMP~".~"TEDTM~",~"QTEMP~".~"BOMTEMP~".~"TEDTE~"
        


"  sort="tcalculate D cls A srce A tcitn A twkct A " )
