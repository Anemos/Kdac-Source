 retrieve="select a.twcmcd,a.twplant,a.twdvsn,a.tworct,a.twitno,a.twochk,
         a.twitnm,a.twpdcd,a.twpdnm,a.twitcl,a.twsrce,
         a.twunit,a.twbgqt,a.twinqt,a.twusqt,a.twprno,
         a.twprnm,a.twvrno,a.twvrnm,a.twprqt,a.twchqt,a.twohqt,a.twqty,decimal(b.wbavrg1 * c.convqty,15,2) as wbavrg1
  from qtemp.wiptemp01 a left outer join pbwip.wip002 b
	on a.twcmcd = b.wbcmcd and a.twplant = b.wbplant and
		a.twdvsn = b.wbdvsn and a.tworct = b.wborct and
		a.twitno = b.wbitno and b.wbiocd <> '3' and
		b.wbyear = :ag_year and b.wbmonth = :ag_month
	inner join pbinv.inv101 c
	on b.wbcmcd = c.comltd and b.wbplant = c.xplant and
		b.wbdvsn = c.div and b.wbitno = c.itno
  order by a.twpdcd,a.tworct,a.twitno,a.twochk" arguments=(("ag_year", string),("ag_month", string)) )
