 retrieve="  SELECT ~"PBBPM~".~"BPM001~".~"PCITN~",   
         ~"PBBPM~".~"BPM001~".~"PEDTE~", 
         ~"PBBPM~".~"BPM001~".~"PLANT~",  
         ~"PBBPM~".~"BPM001~".~"PDVSN~",   
         ~"PBBPM~".~"BPM001~".~"PPITN~"  
    FROM ~"PBBPM~".~"BPM001~"  
   WHERE ( ~"PBBPM~".~"BPM001~".~"PCMCD~" = '01' ) AND
			( ~"PBBPM~".~"BPM001~".~"PLANT~" = :A_PLANT ) AND
         ( ~"PBBPM~".~"BPM001~".~"PDVSN~" = :A_DIV ) AND  
         ( ~"PBBPM~".~"BPM001~".~"PPITN~" = :A_ITNO ) AND  
         ( (~"PBBPM~".~"BPM001~".~"PEDTM~" <= ~"PBBPM~".~"BPM001~".~"PEDTE~" AND ~"PBBPM~".~"BPM001~".~"PEDTE~" <> ' '
            AND ~"PBBPM~".~"BPM001~".~"PEDTE~" >= :a_date ) 
          OR  
         ( ~"PBBPM~".~"BPM001~".~"PEDTE~" = ' ' ))
" arguments=(("A_PLANT", string),("A_DIV", string),("A_ITNO", string),("A_DATE", string)) )
