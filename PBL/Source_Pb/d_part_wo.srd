 retrieve="  SELECT wo_code,   
         wo_base,   
         wo_descript,   
         wo_priorty,   
         wo_outorder,   
         wo_mh,   
         wo_esttime_hour,   
         wo_esttime_minute,   
         wo_div_code,   
         wo_state_code,   
         wo_float_date,   
         wo_estend_date,   
         wo_acc_date,   
         wo_start_date,   
         wo_end_date,   
         wo_firm_time_hour,   
         wo_firm_time_minute,   
         wo_time_hour,   
         wo_time_minute,   
         wo_cause,   
         wo_status,   
         cast(wo_action  as varchar(500)) as wo_action,   
         comp_code,   
         wo_value,   
         A.equip_code,   
         A.area_code,   
         A.factory_code,   
         B.equip_name  as equip_NAME,
			b.cc_code
    FROM wo A left outer join equip_master  B
		on A.area_code = B.area_code and
			A.factory_code = B.factory_code and
			A.equip_code = B.equip_code
   WHERE a.wo_float_date > dateadd(day,-7,getdate()) 
			and a.area_code = :aaa and a.factory_code=:bbb
union
  SELECT wo_code,   
         wo_base,   
         wo_descript,   
         wo_priorty,   
         wo_outorder,   
         wo_mh,   
         wo_esttime_hour,   
         wo_esttime_minute,   
         wo_div_code,   
         wo_state_code,   
         wo_float_date,   
         wo_estend_date,   
         wo_acc_date,   
         wo_start_date,   
         wo_end_date,   
         wo_firm_time_hour,   
         wo_firm_time_minute,   
         wo_time_hour,   
         wo_time_minute,   
         wo_cause,   
         wo_status,   
         cast(wo_action  as varchar(500)) as wo_action,   
         comp_code,   
         wo_value,   
         A.equip_code,   
         A.area_code,   
         A.factory_code,   
         B.equip_name  as equip_NAME,
			b.cc_code
    FROM wo_hist A left outer join equip_master B 
		on A.area_code = B.area_code and
			A.factory_code = B.factory_code and
			A.equip_code = B.equip_code 
   WHERE a.wo_float_date > dateadd(day,-7,getdate()) 
			and a.area_code = :aaa and a.factory_code=:bbb
order by wo_float_date desc   " arguments=(("aaa", string),("bbb", string)) )
