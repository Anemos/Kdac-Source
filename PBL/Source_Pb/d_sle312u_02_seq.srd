 retrieve="  SELECT ~"PBSLE~".~"SLE501~".~"COMLTD~",   
         ~"PBSLE~".~"SLE501~".~"CSRNO~",   
         ~"PBSLE~".~"SLE501~".~"CSRNO1~",   
         ~"PBSLE~".~"SLE501~".~"CSRNO2~",   
