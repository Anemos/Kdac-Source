         ~"PBSLE~".~"SLE501~".~"CNLCD~",   
         ~"PBSLE~".~"SLE501~".~"SUSE~",   
         ~"PBSLE~".~"SLE501~".~"CUR~",   
         ~"PBSLE~".~"SLE501~".~"LUPRC~",   
         ~"PBSLE~".~"SLE501~".~"DCQTY~",   
         ~"PBSLE~".~"SLE501~".~"STCD~",   
         ~"PBSLE~".~"SLE501~".~"SJCD~",
		   ~"PBSLE~".~"SLE501~".~"UPDTDT~"
    FROM ~"PBSLE~".~"SLE101~",   
         ~"PBSLE~".~"SLE501~"
   WHERE ( ~"PBSLE~".~"SLE101~".~"COMLTD~" 	= '01' ) AND  
         ( ~"PBSLE~".~"SLE501~".~"DTYPE~"  	= 'C' ) AND  
         ( ~"PBSLE~".~"SLE501~".~"CSRNO~"  	= :a_csrno )  and    
         ( ~"PBSLE~".~"SLE501~".~"CSRNO1~" 	= :a_csrno1 )  and    
         ( ~"PBSLE~".~"SLE501~".~"CSRNO2~" 	= :a_csrno2 )  and    
         ( ~"PBSLE~".~"SLE101~".~"COMLTD~" 	= ~"PBSLE~".~"SLE501~".~"COMLTD~" ) and  
         ( ~"PBSLE~".~"SLE101~".~"CUSTCD~" 	= ~"PBSLE~".~"SLE501~".~"CUSTCD~" ) and  
         ( ~"PBSLE~".~"SLE101~".~"CITNO~" 	= ~"PBSLE~".~"SLE501~".~"CITNO~" )
