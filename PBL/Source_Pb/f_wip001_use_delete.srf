global function integer f_wip001_use_delete (string ag_comltd, string ag_sliptype, string ag_srno, string ag_srno1, string ag_srno2, transaction ag_transaction, ref string ag_message);integer li_rtn

// 사용내역삭제
select count(*) into :li_rtn
from pbwip.wip004
where wdcmcd = :ag_comltd and wdprsrty = :ag_sliptype and wdprsrno = :ag_srno and
		wdprsrno1 = :ag_srno1 and wdprsrno2 = :ag_srno2  using ag_transaction;

if li_rtn > 0 then
	// 재공 사용량 조정
	DECLARE up_wip08 PROCEDURE FOR PBWIP.SP_WIP_08  
			A_COMLTD = :ag_comltd,   
			A_PRSRTY = :ag_sliptype,   
			A_PRSRNO = :ag_srno,   
			A_PRSRNO1 = :ag_srno1,   
			A_PRSRNO2 = :ag_srno2  using ag_transaction;
	
	execute up_wip08;		
end if

if ag_transaction.sqlcode = -1 then
	ag_message = "재공사용 내역삭제에러 : " + ag_transaction.sqlerrtext
	return -1
end if

return 0
end function

