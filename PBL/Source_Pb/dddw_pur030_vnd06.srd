 retrieve=" SELECT 1 as gubun, '' as vndnm,'' as vsrno,'' as vndr	
 FROM ~"PBPUR~".~"PURDUM~"
 union	  
  SELECT 2 as gubun,
			~"PBPUR~".~"PUR101~".~"VNDNM~",   
         ~"PBPUR~".~"PUR101~".~"VSRNO~",   
         ~"PBPUR~".~"PUR101~".~"VNDR~"  
    FROM ~"PBPUR~".~"PUR101~"  
   WHERE ( ~"PBPUR~".~"PUR101~".~"SCGUBUN~" = 'S' ) AND  
         ( ~"PBPUR~".~"PUR101~".~"DIGUBUN~" = 'I' ) AND  
         ( ~"PBPUR~".~"PUR101~".~"COMLTD~" = '01' )   
ORDER BY 1,2 ASC   
            
" )
