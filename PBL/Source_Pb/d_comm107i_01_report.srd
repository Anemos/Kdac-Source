         ~"PBCOMMON~".~"COMM121~".~"INPT_ID~",   
         ~"PBCOMMON~".~"COMM121~".~"INPT_DT~",   
         ~"PBCOMMON~".~"COMM121~".~"UPDT_ID~",   
         ~"PBCOMMON~".~"COMM121~".~"UPDT_DT~",   
         ~"PBCOMMON~".~"COMM121~".~"IP_ADDR~",   
         ~"PBCOMMON~".~"COMM121~".~"MAC_ADDR~",   
         ~"PBCOMMON~".~"COMM140~".~"USE_WIN~",   
         ~"PBCOMMON~".~"DAC003~".~"PENAMEK~",   
         ~"PBCOMMON~".~"COMM140~".~"USE_GRD~",   
         ~"PBCOMMON~".~"DAC003~".~"PECLASS~" , 
         ~"PBCOMMON~".~"DAC002~".~"COITNAME~",
         current timestamp as cur_Date,
         coalesce(b.win_name,'')
    FROM ~"PBCOMMON~".~"COMM121~",   
         ~"PBCOMMON~".~"DAC003~" ,
         ~"PBCOMMON~".~"DAC002~" ,
         ~"PBCOMMON~".~"DAC001~" ,
			~"PBCOMMON~".~"COMM140~" left outer join ( select win_id,max(win_nm) as win_name from ~"PBCOMMON~".~"COMM110~"   
                                                   group by win_id ) b
			on ( ~"PBCOMMON~".~"COMM140~".~"USE_WIN~" = b.win_id )
   WHERE ( ~"PBCOMMON~".~"COMM121~".~"EMP_NO~" = ~"PBCOMMON~".~"COMM140~".~"EMP_NO~" ) and  
         ( ~"PBCOMMON~".~"COMM121~".~"EMP_NO~" = ~"PBCOMMON~".~"DAC003~".~"PEEMPNO~" ) and
         ( ~"PBCOMMON~".~"COMM140~".~"USE_GRD~" LIKE :ag_grade ) AND
         ( ~"PBCOMMON~".~"DAC003~".~"PEDEPT~" LIKE :ag_Dept ) and 
         ( ~"PBCOMMON~".~"COMM140~".~"USE_WIN~" LIKE :ag_pgmid ) AND
       	( ~"PBCOMMON~".~"DAC002~".~"COMLTD~"  = '01') and
			( ~"PBCOMMON~".~"DAC002~".~"COGUBUN~" = 'PER306')  and
			( ~"PBCOMMON~".~"DAC002~".~"COCODE~"  =  ~"PBCOMMON~".~"DAC003~".~"PECLASS~" ) and
         ( ~"PBCOMMON~".~"DAC001~".~"DCODE~" = ~"PBCOMMON~".~"DAC003~".~"PEDEPT~" 
				 AND ~"PBCOMMON~".~"DAC001~".~"DUSE~" = ' '
				 AND ~"PBCOMMON~".~"DAC001~".~"DTODT~" = 0
				 AND ~"PBCOMMON~".~"DAC001~".~"DACTTODT~" = 0 )
