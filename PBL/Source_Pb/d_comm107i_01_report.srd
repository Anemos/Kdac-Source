 retrieve="  SELECT ~"PBCOMMON~".~"DAC001~".~"DNAME~",
         ~"PBCOMMON~".~"COMM121~".~"EMP_NO~",   
         ~"PBCOMMON~".~"COMM121~".~"USESTS~",   
         ~"PBCOMMON~".~"COMM121~".~"AUTAREA~",   
         ~"PBCOMMON~".~"COMM121~".~"AUTPLNT~",   
         ~"PBCOMMON~".~"COMM121~".~"AUTDIV~",   
         ~"PBCOMMON~".~"COMM121~".~"AUTEXT1~",   
