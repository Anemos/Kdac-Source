 retrieve="  SELECT TSeqMstItem.CustomerCode,   
         TSeqMstItem.CustomerItemCode,   
         TSeqMstItem.AreaCode,   
         TSeqMstItem.DivisionCode,   
         TSeqMstItem.Itemcode,   
         TSeqMstItem.PartId,   
			TSeqMstItem.PartId2, 
         TSeqMstItem.ProductGroup,   
         TSeqMstItem.LineCode,   
         TSeqMstItem.CarType,  
			TSeqMstItem.Assygubun, 
         TSeqMstItem.Seqgubun,
         TSeqMstItem.LotSize,   
         TSeqMstItem.SafeStockQty,   
         TSeqMstItem.LastEmp,   
         TSeqMstItem.LastDate,   
         '1            '  as pt_chk  ,
			TMstArea.AreaName,
			TMstDivision.DivisionName,
			TMstcustitem.Areacode as xplant,
			TMstcustitem.Divisioncode as div
    FROM TSeqMstItem   , TMstArea , TMstDivision , TMstcustitem
	where TSeqMstItem.AreaCode = TMstArea.AreaCode     and
			TSeqMstItem.AreaCode = TMstDivision.AreaCode     and
			TSeqMstItem.Divisioncode = TMstDivision.DivisionCode     and
			TSeqMstItem.CustomerCode = :a_customercode 	and
         TSeqMstItem.CustomerItemCode = :a_customerItemcode    
			 
" update="TSeqMstItem" updatewhere=0 updatekeyinplace=no arguments=(("a_customercode", string),("a_customeritemcode", string)) )
