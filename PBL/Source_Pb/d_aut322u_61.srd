 retrieve="  SELECT A.COMLTD,   
         A.XPLANT,   
         A.DIV,   
         A.DEPT,   
         A.SWLGUBUN,   
         A.SWVGUBUN,   
         A.SWNM,   
         A.QTY,   
         A.COMMENT,   
         A.INPTID,   
         A.INPTDT,   
         A.UPDTID,   
         A.UPDTDT,   
         A.IPADDR,   
         A.MACADDR  
    FROM PBCOMMON.COMM815 AS A  
WHERE A.COMLTD = '01' AND
		A.XPLANT   LIKE :as_xplant AND
		A.DIV      LIKE :as_div AND
		A.DEPT     LIKE :as_dept AND
		A.SWLGUBUN LIKE :as_swlgubun AND
		A.SWVGUBUN LIKE :as_swvgubun 
ORDER BY A.XPLANT, A.DIV, A.DEPT, A.SWLGUBUN, A.SWVGUBUN

" update="PBCOMMON.COMM815" updatewhere=1 updatekeyinplace=no arguments=(("as_xplant", string),("as_div", string),("as_dept", string),("as_swlgubun", string),("as_swvgubun", string)) )
