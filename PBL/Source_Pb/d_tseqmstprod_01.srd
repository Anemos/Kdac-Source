 retrieve="  SELECT TSeqMstProd.ProductGroup,   
         TSeqMstProd.ProductName,   
         TSeqMstProd.SeqGubun,   
         TSeqMstProd.LastEmp,   
         TSeqMstProd.LastDate,   
         TSeqMstProd.Areacode,   
         TSeqMstProd.Divisioncode,   
			TMstDivision.DivisionName,
         '1   ' as pt_chk  
    FROM TSeqMstProd  , TMstDivision
   WHERE ( TSeqMstProd.Areacode = :a_areacode ) AND  
         ( TSeqMstProd.Divisioncode like :a_divisioncode )    and
			( TSeqMstProd.Divisioncode = TMstDivision.Divisioncode )    and
			( TSeqMstProd.Areacode = TMstDivision.Areacode )    
" update="TSeqMstProd" updatewhere=1 updatekeyinplace=no arguments=(("a_areacode", string),("a_divisioncode", string)) )
