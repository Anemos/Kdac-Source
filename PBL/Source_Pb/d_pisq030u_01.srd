 retrieve="  SELECT TQQCSTANDARD.AREACODE,   
         TQQCSTANDARD.DIVISIONCODE,   
         TQQCSTANDARD.SUPPLIERCODE,   
         TQQCSTANDARD.ITEMCODE,   
         TQQCSTANDARD.STANDARDREVNO,   
         TQQCSTANDARD.CHARGENAME,   
         TQQCSTANDARD.ENACTMENTDATE,   
         TQQCSTANDARD.ITEMRANK,   
         TQQCSTANDARD.QUALITY,   
         TQQCSTANDARD.HEATTREATMENT,   
         TQQCSTANDARD.SURFACETREATMENT,
			TQQCSTANDARD.QCCONCEPTIONREFERENCE,   
         TQQCSTANDARD.BADTREATMENT,   
         TQQCSTANDARD.CONSERTDEPENDENCEFLAG,   
         TQQCSTANDARD.CHARGECODE,   
         TQQCSTANDARD.CHARGECONSERTFLAG,   
         TQQCSTANDARD.SANCTIONCODE,   
         TQQCSTANDARD.SANCTIONCONSERTFLAG,   
         TQQCSTANDARD.CONSERTDATE,   
         TQQCSTANDARD.RETURNREASON,   
         TQQCSTANDARD.MODIFYCONTENT,   
         TQQCSTANDARD.APPLYDATE,   
         TQQCSTANDARD.CHANGEFLAG,   
         TMSTAREA.AreaName,   
         TMSTDIVISION.DivisionName,   
         TMSTITEM.ItemName,  
         TMSTSUPPLIER.SupplierKorName  
    FROM TQQCSTANDARD,   
         TMSTAREA,   
         TMSTDIVISION,   
         TMSTITEM,   
         TMSTSUPPLIER  
   WHERE ( TQQCSTANDARD.AREACODE *= TMSTAREA.AreaCode ) and  
         ( TQQCSTANDARD.AREACODE *= TMSTDIVISION.AreaCode ) and  
         ( TQQCSTANDARD.DIVISIONCODE *= TMSTDIVISION.DivisionCode ) and  
         ( TQQCSTANDARD.ITEMCODE *= TMSTITEM.ItemCode ) and  
         ( TQQCSTANDARD.SUPPLIERCODE *= TMSTSUPPLIER.SupplierCode ) and  
         ( TQQCSTANDARD.AREACODE = :as_areacode ) AND  
         ( TQQCSTANDARD.DIVISIONCODE = :as_divisioncode ) AND  
         ( TQQCSTANDARD.SUPPLIERCODE = :as_suppliercode ) AND  
         ( TQQCSTANDARD.ITEMCODE = :as_itemcode ) AND  
         ( TQQCSTANDARD.STANDARDREVNO = :as_standardrevno ) 
 " update="TQQCSTANDARD" updatewhere=1 updatekeyinplace=no arguments=(("as_areacode", string),("as_divisioncode", string),("as_suppliercode", string),("as_itemcode", string),("as_standardrevno", string)) )
