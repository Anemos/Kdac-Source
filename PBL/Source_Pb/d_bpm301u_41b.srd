 retrieve="  SELECT a.YITNO,
         (select coalesce(max(itnm),'') from pbbpm.bpm502 x
          where x.xyear = a.yccyy
          and   x.revno = a.revno
          and   x.itno = a.yitno) itnm,   
         (select coalesce(max(spec),'') from pbbpm.bpm502 x
          where x.xyear = a.yccyy
          and   x.revno = a.revno
          and   x.itno = a.yitno) spec,   
         a.YVNDSR,   
         a.YPLANT,   
         a.YDIV,   
         a.YGUBUN,   
         a.YGRAD,   
         a.YSRCE,   
         a.YPLAN,
          (select coalesce(max(x.coitname),'') from pbcommon.dac002 x
          where x.comltd = '01'
          and   x.cogubun = (case when a.ysrce = '01' then 'INV060' else 'INV050' end)
          and   x.cocode = a.yplan) xplannm,    
         a.YCLSB,   
         a.YPDCD,   
         a.BOMUNIT,   
         a.XUNIT,   
         a.XUNIT1,   
         a.CONVINV,   
         a.CONVPUR,   
         a.CONVQTY,   
         a.YCHGE,   
         a.YCURR,   
         a.YCSTC,   
         a.YADJDT,   
         a.YCSTR,   
         (case when a.ysrce = '01' then ( case when yplant = 'Y' then
													(select coalesce(max(dper),0) from pbbpm.bpm507 x
													  where x.xyear = a.yccyy
                                         and   x.revno = a.revno
													  and   x.gubun = 'A'
													  and   x.xplant = a.yplant) 
												 else
												   (select coalesce(max(dper),0) from pbbpm.bpm507 x
													 where x.xyear = a.yccyy
                                        and   x.revno = a.revno
													 and   x.gubun = 'B'
													 and   x.xplant = a.ycode
													 and   x.div = '') end) 
                                  else 0 end) ydper,
         (case when a.ysrce = '01' then ( case when yplant = 'Y' then
													(select coalesce(max(eper),0) from pbbpm.bpm507 x
													  where x.xyear = a.yccyy
                                         and   x.revno = a.revno
													  and   x.gubun = 'A'
													  and   x.xplant = a.yplant) 
												 else
												   (select coalesce(max(eper),0) from pbbpm.bpm507 x
													 where x.xyear = a.yccyy
                                        and   x.revno = a.revno
													 and   x.gubun = 'B'
													 and   x.xplant = a.ycode
													 and   x.div = '') end) 
                                  else 0 end) yeper,
         (select coalesce(max(exch),1) from pbbpm.bpm506 x
						   where  x.xyear = a.yccyy
                     and   x.revno = a.revno  
							and    x.curr = a.ycurr) exch,  
         a.YCSTD,   
         a.YCSTE,  
         a.YCSTD1,   
         a.YCSTE1,  
         a.YGCST,   
         a.YCODE,   
         a.YALT,   
         a.YEXPT,   
         a.YCODE1,   
         a.YCMCD,   
         a.YDATE,   
         a.YREDT,   
         a.UPDTID,   
         a.UPDTDT,   
         a.CRTDT  
   FROM PBBPM.BPM509 a 
   WHERE ( a.YCCYY = :as_xyear )   
   and   (a.revno =  :as_revno )
   and   ( a.YPLANT like :as_xplant || '%' )   
   and   ( a.YDIV like :as_div || '%' )   
   and   (a.ysrce = '01' and (select count(*) from pbbpm.bpm509 x
                              where  x.yccyy = a.yccyy
                              and   x.revno = a.revno
                              and    x.ygubun = '10'
                              and    x.yitno = a.yitno
                              and    x.ysrce <> '01') > 0
          )
" arguments=(("as_xyear", string),("as_revno", string),("as_xplant", string),("as_div", string)) )
