 retrieve="   SELECT ~"PBBPM~".~"BPM003~".~"OPLANT~", 
         ~"PBBPM~".~"BPM003~".~"ODVSN~",   
         ~"PBBPM~".~"BPM003~".~"OPITN~",   
         ~"PBBPM~".~"BPM003~".~"OFITN~",   
         ~"PBBPM~".~"BPM003~".~"OCHDT~",   
         ~"PBBPM~".~"BPM003~".~"OEDTM~",   
         ~"PBBPM~".~"BPM003~".~"OEDTE~"  
    FROM ~"PBBPM~".~"BPM003~"  
   WHERE ( ~"PBBPM~".~"BPM003~".~"OCMCD~" = '01' ) AND
			( ~"PBBPM~".~"BPM003~".~"OPLANT~" = :a_plant ) AND 
         ( ~"PBBPM~".~"BPM003~".~"ODVSN~" = :a_div ) AND  
         ( ~"PBBPM~".~"BPM003~".~"OPITN~" = :a_opitn ) AND  
         ( ~"PBBPM~".~"BPM003~".~"OFITN~" = :a_ofitn ) AND  
         ( (~"PBBPM~".~"BPM003~".~"OEDTM~" <= ~"PBBPM~".~"BPM003~".~"OEDTE~" AND ~"PBBPM~".~"BPM003~".~"OEDTE~" <> ' ' AND ~"PBBPM~".~"BPM003~".~"OEDTE~" >= :a_date )
          OR  
         ( ~"PBBPM~".~"BPM003~".~"OEDTE~" = ' ' )) 
ORDER BY ~"PBBPM~".~"BPM003~".~"OEDTM~" ASC,   
         ~"PBBPM~".~"BPM003~".~"OEDTE~" ASC   
" arguments=(("a_plant", string),("a_div", string),("a_opitn", string),("a_ofitn", string),("a_date", string)) )
