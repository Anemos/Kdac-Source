 retrieve="  SELECT A.COMLTD,   
         A.CHID,
			A.CHGDT,   
         A.ASID,   
         A.ASDT,   
         A.XPLANT,   
         A.DIV,   
         A.DEPT,   
         A.DEVICEGUBUN,   
         A.SPECGUBUN,   
         A.MODELNM,   
         A.MEMORY,   
         A.DISK,   
         A.CDRWEXIST,   
         A.MONITORSIZE,   
         A.MONITORGUBUN,   
         A.OS,   
         A.EMPGUBUN,   
         A.EMPNM,
			A.USBEXIST,   
         A.STCD,
			A.COMMENT,
			A.ETC,
			A.INPTID,   
         A.INPTDT,   
         A.UPDTID,   
         A.UPDTDT,   
         A.IPADDR,   
         A.MACADDR,
			A.USINGIP  
    FROM PBCOMMON.COMM810  AS A
   WHERE A.COMLTD       = '01'  	   AND
			A.DEPT      LIKE :as_dept  AND
			A.DEVICEGUBUN IN ('N','D')	AND
			A.STCD        IN ('I','P')       
            
ORDER BY A.XPLANT, A.DIV, A.DEPT, A.CHGDT, A.CHID
" update="PBCOMMON.COMM810" updatewhere=0 updatekeyinplace=no arguments=(("as_dept", string)) )
