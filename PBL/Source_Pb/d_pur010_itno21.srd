 retrieve="SELECT C.RROGB, C.ITNO, C.ITNM, C.SPEC, C.XUNIT, C.MAKER, C.XPLAN, C.EXTD
FROM (
		SELECT A.COMLTD,A.ITNO,A.ITNM,A.SPEC,A.XUNIT,A.MAKER,A.XPLAN, A.RROGB, A.EXTD
		FROM (
				SELECT COMLTD,ITNO,ITNM,SPEC,XUNIT,MAKER,XPLAN, RROGB, EXTD
				FROM PBINV.INV002 
				WHERE COMLTD = '01'
				 AND INL <> ' '
				 AND GUBUN = '0'
				) A EXCEPTION JOIN (SELECT COMLTD, ITNO FROM PBINV.INV101
										  WHERE COMLTD = '01' 
										  GROUP BY COMLTD, ITNO ) B
		ON A.COMLTD = B.COMLTD
		AND A.ITNO = B.ITNO 
      ) C
WHERE C.COMLTD = '01'
 AND C.ITNO LIKE :ag_itno" arguments=(("ag_itno", string))  sort="itno A " )
