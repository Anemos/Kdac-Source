 retrieve="  SELECT ~"PBPDM~".~"BOM113~".~"ZCMCD~",   
         ~"PBPDM~".~"BOM113~".~"ZDATE~",   
         ~"PBPDM~".~"BOM113~".~"ZPLANT~",   
         ~"PBPDM~".~"BOM113~".~"ZDIV~",   
         ~"PBPDM~".~"BOM113~".~"ZMDCD~",   
         ~"PBPDM~".~"BOM113~".~"ZMDNO~",   
         ~"PBPDM~".~"BOM113~".~"ZSERIAL~",   
         ~"PBPDM~".~"BOM113~".~"ZLEVEL~",   
         ~"PBPDM~".~"BOM113~".~"ZITNO~",   
         ~"PBPDM~".~"BOM113~".~"ZREV~",   
         ~"PBPDM~".~"BOM113~".~"ZDESC~",   
         ~"PBPDM~".~"BOM113~".~"ZSPEC~",   
         ~"PBPDM~".~"BOM113~".~"ZUNIT1~",   
         ~"PBPDM~".~"BOM113~".~"ZUNITQTY~",   
         ~"PBPDM~".~"BOM113~".~"ZCVTFACT~",   
         ~"PBPDM~".~"BOM113~".~"ZUNIT2~",   
         ~"PBPDM~".~"BOM113~".~"ZABCCODE~",   
         ~"PBPDM~".~"BOM113~".~"ZISSCODE~",   
         ~"PBPDM~".~"BOM113~".~"ZWHLCODE~",   
         ~"PBPDM~".~"BOM113~".~"ZLOTSIZE~",   
         ~"PBPDM~".~"BOM113~".~"ZLEADTIME~",   
         CASE WHEN ~"PBPDM~".~"BOM113~".~"ZYGCHK~" = 'Y' 
				THEN ~"PBPDM~".~"BOM113~".~"ZSCOUTCOST~"
				ELSE ~"PBPDM~".~"BOM113~".~"ZOUTCOST~" END AS ZMOVCOST,   
			CASE WHEN ~"PBPDM~".~"BOM113~".~"ZYGCHK~" = 'Y' 
				THEN ~"PBPDM~".~"BOM113~".~"ZSCRCPCOST~"
				ELSE ~"PBPDM~".~"BOM113~".~"ZRCPCOST~" END AS ZRCPCOST, 
			~"PBPDM~".~"BOM113~".~"ZOUTCOST~", 
			~"PBPDM~".~"BOM113~".~"ZSCMOVCOST~",   
         ~"PBPDM~".~"BOM113~".~"ZSCRCPCOST~", 
			~"PBPDM~".~"BOM113~".~"ZSCOUTCOST~",
         ~"PBPDM~".~"BOM113~".~"ZWKCT~",   
         ~"PBPDM~".~"BOM113~".~"ZGRAD~",   
         ~"PBPDM~".~"BOM113~".~"ZALTNO~",   
         ~"PBPDM~".~"BOM113~".~"ZFMDT~",   
         ~"PBPDM~".~"BOM113~".~"ZTODT~", 
			~"PBPDM~".~"BOM113~".~"ZEXPLANT~",
			~"PBPDM~".~"BOM113~".~"ZEXDIV~",  
         CASE WHEN ~"PBINV~".~"INV402~".~"SRCE~" = '03' THEN 'N' ELSE 'Y' END AS ZCALC,
			~"PBPDM~".~"BOM113~".~"ZYGCHK~",   
         ~"PBINV~".~"INV402~".~"CLS~",   
         ~"PBINV~".~"INV402~".~"SRCE~",   
         ~"PBCOMMON~".~"DAC007~".~"PRNAME~",
			A.COITNAME AS PLANTNAME,
			B.COITNAME AS DIVNAME,
			~"PBPDM~".~"BOM113~".~"ZCOMCD~",
			:ps_applygubun AS APPLYGUBUN,
			~"PBPDM~".~"BOM113~".~"ZCOSTCHK~"
    FROM ~"PBPDM~".~"BOM113~",   
         ~"PBINV~".~"INV402~",
         ~"PBCOMMON~".~"DAC007~",  
         ~"PBCOMMON~".~"DAC002~" A, 
         ~"PBCOMMON~".~"DAC002~" B  
   WHERE ( ~"PBPDM~".~"BOM113~".~"ZCMCD~" 	= ~"PBINV~".~"INV402~".~"COMLTD~" ) and  
         ( ~"PBPDM~".~"BOM113~".~"ZEXPLANT~" 	= ~"PBINV~".~"INV402~".~"XPLANT~" ) and  
         ( ~"PBPDM~".~"BOM113~".~"ZEXDIV~" 		= ~"PBINV~".~"INV402~".~"DIV~" ) and  
         ( ~"PBPDM~".~"BOM113~".~"ZITNO~" 	= ~"PBINV~".~"INV402~".~"ITNO~" ) and  
         ( ~"PBPDM~".~"BOM113~".~"ZDATE~" 	= ~"PBINV~".~"INV402~".~"XYEAR~" ) and   
         ( ~"PBPDM~".~"BOM113~".~"ZCMCD~" 	= ~"PBCOMMON~".~"DAC007~".~"COMLTD~" ) and  
         ( ~"PBPDM~".~"BOM113~".~"ZMDCD~" 	= ~"PBCOMMON~".~"DAC007~".~"PRPRCD~" ) and 
         ( ( ~"PBPDM~".~"BOM113~".~"ZCMCD~" = '01' ) AND  
         ( ~"PBPDM~".~"BOM113~".~"ZDATE~" = :ps_year ) AND  
         ( ~"PBPDM~".~"BOM113~".~"ZPLANT~" like :ps_xplant ) AND  
         ( ~"PBPDM~".~"BOM113~".~"ZDIV~" like :ps_div ) AND  
         ( trim(~"PBPDM~".~"BOM113~".~"ZMDCD~") like :ps_pdcd ) AND  
         ( trim(~"PBPDM~".~"BOM113~".~"ZMDNO~") like :ps_itno ) )  AND
         ( ~"PBPDM~".~"BOM113~".~"ZCMCD~" = A.COMLTD AND ~"PBPDM~".~"BOM113~".~"ZPLANT~" = TRIM(A.COCODE) AND A.COGUBUN = 'SLE220' ) AND
			( ~"PBPDM~".~"BOM113~".~"ZCMCD~" = B.COMLTD AND ~"PBPDM~".~"BOM113~".~"ZDIV~"   = TRIM(B.COCODE) AND B.COGUBUN = 'EIS040' )  
" arguments=(("ps_year", string),("ps_xplant", string),("ps_div", string),("ps_pdcd", string),("ps_itno", string),("ps_applygubun", string))  sort="zplant A zdiv A zmdcd A zmdno A zserial A " )
