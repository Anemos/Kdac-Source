		         A.AMOUNT3          ,
		         A.AMOUNT4          ,
		         A.AMOUNT5          ,
		         A.AMOUNT6          ,
		         A.AMOUNT7          ,
		         A.AMOUNT8          ,
		         A.AMOUNT9          ,
		         A.AMOUNT10         ,
		         A.AMOUNT11         ,
