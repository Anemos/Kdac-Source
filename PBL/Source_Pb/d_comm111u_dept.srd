 retrieve="SELECT  	'1' AS ORDER_NO,PBCOMMON.DAC001.DCODE ,
           	trim(PBCOMMON.DAC001.DNAME) || ' ( ' || dcode ||  ' )' as deptname ,
           	'N' as sel_col    
FROM 	PBCOMMON.DAC001      
WHERE 	PBCOMMON.DAC001.DTODT = 0  	and	PBCOMMON.DAC001.DUSE		=	' '	and          
			PBCOMMON.DAC001.DSYS1 = 'X' 	and	(PBCOMMON.DAC001.DLEVEL2 	= '3' or ( PBCOMMON.DAC001.DLEVEL2  = '4' and PBCOMMON.DAC001.DUNIT = '1' ))	and 
           	PBCOMMON.DAC001.DNAME like :ag_deptname    
Union 
SELECT   '2',PBCOMMON.COMM111.DEPT_CD,PBCOMMON.COMM111.DEPT_CD,
           	'N' as sel_col    
FROM 	PBCOMMON.COMM111      
WHERE 	PBCOMMON.COMM111.DEPT_CD NOT IN 
			(	SELECT  	PBCOMMON.DAC001.DCODE
				FROM 	PBCOMMON.DAC001      
				WHERE 	PBCOMMON.DAC001.DTODT = 0  	and	PBCOMMON.DAC001.DUSE		=	' '	and          
							PBCOMMON.DAC001.DSYS1 = 'X' 	and	(PBCOMMON.DAC001.DLEVEL2 	= '3' or ( PBCOMMON.DAC001.DLEVEL2  = '4' and PBCOMMON.DAC001.DUNIT = '1' ))	and 
								PBCOMMON.DAC001.DNAME like :ag_deptname   )

