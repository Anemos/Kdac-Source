 retrieve="  SELECT ~"PBBPM~".~"BPM001~".~"PCITN~",   
         ~"PBBPM~".~"BPM001~".~"PQTYM~",   
         ~"PBBPM~".~"BPM001~".~"PWKCT~",   
         ~"PBBPM~".~"BPM001~".~"PEDTM~",   
         ~"PBBPM~".~"BPM001~".~"PEDTE~",   
         ~"PBBPM~".~"BPM001~".~"PPITN~",   
         ~"PBBPM~".~"BPM001~".~"PDVSN~",
			~"PBBPM~".~"BPM001~".~"PLANT~",   
         ~"PBBPM~".~"BPM001~".~"PROUT~",   
         ~"PBBPM~".~"BPM001~".~"PCHDT~",
			~"PBBPM~".~"BPM001~".~"PEXPLANT~",   
         ~"PBBPM~".~"BPM001~".~"PEXDV~",   
         ~"PBBPM~".~"BPM001~".~"PCHCD~",   
         ~"PBBPM~".~"BPM001~".~"POSCD~",   
         ~"PBBPM~".~"BPM001~".~"PINDT~",   
         ~"PBBPM~".~"BPM001~".~"PEMNO~",   
         ~"PBBPM~".~"BPM001~".~"POPCD~",   
         ~"PBBPM~".~"BPM001~".~"PEBST~",   
         ~"PBBPM~".~"BPM001~".~"PQTYE~",
         ~"PBINV~".~"INV101~".~"CLS~",
         ~"PBINV~".~"INV101~".~"SRCE~",
         ~"PBINV~".~"INV002~".~"ITNM~",
         ~"PBINV~".~"INV002~".~"XUNIT~",
         '               ' AS OPT_ITNO   
    FROM ~"PBBPM~".~"BPM001~",~"PBINV~".~"INV002~",~"PBINV~".~"INV101~"  
   WHERE ( ~"PBBPM~".~"BPM001~".~"PCMCD~" = '01' ) AND
			( ~"PBBPM~".~"BPM001~".~"PLANT~" = :A_PLANT ) AND  
         ( ~"PBBPM~".~"BPM001~".~"PDVSN~" = :A_DIV ) AND  
         ( ~"PBBPM~".~"BPM001~".~"PPITN~" = :A_PITNO ) AND  
         ( (~"PBBPM~".~"BPM001~".~"PEDTM~" <= ~"PBBPM~".~"BPM001~".~"PEDTE~" AND ~"PBBPM~".~"BPM001~".~"PEDTE~" <> ' ' AND ~"PBBPM~".~"BPM001~".~"PEDTE~" >= :a_date )
          OR  
         ( ~"PBBPM~".~"BPM001~".~"PEDTE~" = ' ' )) AND
         ( ~"PBBPM~".~"BPM001~".~"PCMCD~" = ~"PBINV~".~"INV101~".~"COMLTD~" ) AND
			( ~"PBBPM~".~"BPM001~".~"PLANT~" = ~"PBINV~".~"INV101~".~"XPLANT~" ) AND  
         ( ~"PBBPM~".~"BPM001~".~"PDVSN~" = ~"PBINV~".~"INV101~".~"DIV~" ) AND  
         ( ~"PBBPM~".~"BPM001~".~"PCITN~" = ~"PBINV~".~"INV101~".~"ITNO~" ) AND  
         ( ~"PBBPM~".~"BPM001~".~"PCMCD~" = ~"PBINV~".~"INV002~".~"COMLTD~" ) AND
			( ~"PBBPM~".~"BPM001~".~"PCITN~" = ~"PBINV~".~"INV002~".~"ITNO~" )
ORDER BY ~"PBBPM~".~"BPM001~".~"PCITN~" ASC,   
         ~"PBBPM~".~"BPM001~".~"PEDTM~" ASC   
" arguments=(("A_PLANT", string),("A_DIV", string),("A_PITNO", string),("A_DATE", string)) )
