         ( ~"PBRTN~".~"RTN016~".~"RFLINE1~" = :a_line1 AND ~"PBRTN~".~"RTN016~".~"RFLINE2~"  = :a_line2 ) AND
         (~"PBRTN~".~"RTN016~".~"RFEDFM~" <= :a_ajdate AND ~"PBRTN~".~"RTN016~".~"RFEDTO~" >= :a_ajdate AND
          ~"PBRTN~".~"RTN016~".~"RFEDFM~" <= ~"PBRTN~".~"RTN016~".~"RFEDTO~" )

