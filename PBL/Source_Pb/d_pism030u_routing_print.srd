         ~"PBRTN~".~"RTN016~".~"RFPLANT~",
         0,0,' '
    FROM ~"PBRTN~".~"RTN016~" , ~"PBRTN~".~"RTN015~"    
    WHERE ( ~"PBRTN~".~"RTN016~".~"RFCMCD~"  = ~"PBRTN~".~"RTN015~".~"RECMCD~" ) AND  
         ( ~"PBRTN~".~"RTN016~".~"RFPLANT~" = ~"PBRTN~".~"RTN015~".~"REPLANT~" ) AND   
