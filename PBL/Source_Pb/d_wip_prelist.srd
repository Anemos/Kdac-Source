 column=(type=char(20) updatewhereclause=yes name=wzdesc dbname="WIP090.WZDESC" )
 retrieve="  SELECT wzplant AS wip_plant, 
			trim(wzcttp) AS wip_gubun,
			SUBSTRING(wzcttp,5,3) AS wip_id,
			wzdesc
    FROM PBWIP.WIP090  
   WHERE ( wzcmcd = '01' ) AND
			( SUBSTRING(wzcttp,5,3) >= '010' ) AND
			( SUBSTRING(wzcttp,5,3) <= '030' )
UNION ALL
  SELECT DISTINCT ' ' AS wip_plant, 
			'ALL' AS wip_gubun,
			'040' AS wip_id,
			wzdesc
    FROM PBWIP.WIP090  
   WHERE ( wzcmcd = '01' ) AND
			( SUBSTRING(wzcttp,5,3) = '040' )
UNION ALL
	SELECT DISTINCT ' ' AS wip_plant, 
			'ALL' AS wip_gubun,
			'010' AS wip_id,
			wzdesc
    FROM PBWIP.WIP090  
   WHERE ( wzcmcd = '01' ) AND
			( SUBSTRING(wzcttp,5,3) = '010' )
ORDER BY wip_id ASC,wip_plant ASC, wip_gubun ASC 
" )
