 retrieve="  SELECT DISTINCT ' ' as sel_chk, 
			'RTN011' as GUBUN,   
			AA.~"RAINEMP~",
			( SELECT IFNULL(BB.~"PENAMEK~",'') FROM ~"PBCOMMON~".~"DAC003~" BB WHERE AA.~"RAINEMP~" = BB.~"PEEMPNO~" ) AS INEMPNAME,
			AA.~"RAINCHK~",
			AA.~"RAINTIME~",
			AA.~"RAPLEMP~",
			( SELECT IFNULL(BB.~"PENAMEK~",'') FROM ~"PBCOMMON~".~"DAC003~" BB WHERE AA.~"RAPLEMP~" = BB.~"PEEMPNO~" ) AS PLEMPNAME,
			AA.~"RAPLCHK~",
			AA.~"RAPLTIME~",
			AA.~"RADLEMP~",
			( SELECT IFNULL(BB.~"PENAMEK~",'') FROM ~"PBCOMMON~".~"DAC003~" BB WHERE AA.~"RADLEMP~" = BB.~"PEEMPNO~" ) AS DLEMPNAME,
			AA.~"RADLCHK~",
			AA.~"RADLTIME~"  
    FROM ~"PBRTN~".~"RTN011~" AA 
   WHERE ( ( AA.~"RAINEMP~" like :ag_inemp AND AA.~"RAINCHK~" = :ag_inchk ) OR   
         ( AA.~"RAPLEMP~" like :ag_inemp AND AA.~"RAPLCHK~" = :ag_plchk ) OR   
         ( AA.~"RADLEMP~" like :ag_inemp ) ) AND  
         ( AA.~"RADLCHK~" = 'N' )

UNION ALL

	  SELECT DISTINCT ' ' as sel_chk, 
			'RTN013' as GUBUN,       
         AA.~"RCINEMP~", 
			( SELECT BB.~"PENAMEK~" FROM ~"PBCOMMON~".~"DAC003~" BB WHERE AA.~"RCINEMP~" = BB.~"PEEMPNO~" ) AS INEMPNAME,
         AA.~"RCINCHK~",   
         AA.~"RCINTIME~",   
         AA.~"RCPLEMP~",
			( SELECT BB.~"PENAMEK~" FROM ~"PBCOMMON~".~"DAC003~" BB WHERE AA.~"RCPLEMP~" = BB.~"PEEMPNO~" ) AS PLEMPNAME,   
         AA.~"RCPLCHK~",   
         AA.~"RCPLTIME~",   
         AA.~"RCDLEMP~", 
			( SELECT BB.~"PENAMEK~" FROM ~"PBCOMMON~".~"DAC003~" BB WHERE AA.~"RCDLEMP~" = BB.~"PEEMPNO~" ) AS DLEMPNAME,   
         AA.~"RCDLCHK~",   
         AA.~"RCDLTIME~"
	FROM ~"PBRTN~".~"RTN013~" AA 
   WHERE ( ( AA.~"RCINEMP~" like :ag_inemp AND AA.~"RCINCHK~" = :ag_inchk ) OR  
         ( AA.~"RCPLEMP~" like :ag_inemp AND AA.~"RCPLCHK~" = :ag_plchk ) OR  
         ( AA.~"RCDLEMP~" like :ag_inemp ) ) AND  
         ( AA.~"RCDLCHK~" = 'N' )  " arguments=(("ag_inemp", string),("ag_inchk", string),("ag_plchk", string))  sort="gubun D rtn011_rainemp A rtn011_raplemp A rtn011_radlemp A " )
