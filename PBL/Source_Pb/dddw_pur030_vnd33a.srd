 retrieve=" SELECT 1,'' as vndnm,'' as vsrno,'' as vndr, '' as natn	
 FROM ~"PBPUR~".~"PURDUM~"
 union	  
 SELECT 2,~"PBPUR~".~"PUR101~".~"VNDNM~",   
         ~"PBPUR~".~"PUR101~".~"VSRNO~",   
         ~"PBPUR~".~"PUR101~".~"VNDR~", 
         ~"PBPUR~".~"PUR101~".~"NATN~" 
 FROM ~"PBPUR~".~"PUR101~",   
      ~"PBPUR~".~"PUR102~"  
 WHERE ( ~"PBPUR~".~"PUR101~".~"COMLTD~" = ~"PBPUR~".~"PUR102~".~"COMLTD~" ) and  
         ( ~"PBPUR~".~"PUR102~".~"VSRNO~" = ~"PBPUR~".~"PUR101~".~"VSRNO~" ) and  
