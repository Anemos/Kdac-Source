 column=(type=char(8) updatewhereclause=yes name=pur103_vsrno dbname="VSRNO" )
 column=(type=char(10) updatewhereclause=yes name=pur101_vndr dbname="VNDR" )
 column=(type=char(12) updatewhereclause=yes name=pur103_itno dbname="ITNO" )
 column=(type=char(8) updatewhereclause=yes name=dadjdt dbname="DADJDT" )
