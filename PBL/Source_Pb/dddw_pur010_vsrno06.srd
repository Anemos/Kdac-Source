 retrieve="  SELECT 1, '' AS VNDNM, '' AS VSRNO, '' AS VNDR, '' as ITNO , ' ' AS DADJDT , ' ' AS XSTOP , ' ' as temp
  FROM ~"PBPUR~".~"PURDUM~"
  UNION
  SELECT 2, ~"PBPUR~".~"PUR101~".~"VNDNM~",   
         ~"PBPUR~".~"PUR103~".~"VSRNO~",   
         ~"PBPUR~".~"PUR101~".~"VNDR~",   
         ~"PBPUR~".~"PUR103~".~"ITNO~" ,
 			~"PBPUR~".~"PUR103~".~"DADJDT~" ,
         ~"PBPUR~".~"PUR103~".~"XSTOP~" , ' ' as temp
    FROM ~"PBPUR~".~"PUR103~",   
         ~"PBPUR~".~"PUR101~"  
   WHERE ( ~"PBPUR~".~"PUR103~".~"COMLTD~" = ~"PBPUR~".~"PUR101~".~"COMLTD~" ) and  
         ( ~"PBPUR~".~"PUR103~".~"VSRNO~" = ~"PBPUR~".~"PUR101~".~"VSRNO~" ) and  
         ( ~"PBPUR~".~"PUR103~".~"COMLTD~" = '01' ) AND
			( ~"PBPUR~".~"PUR101~".~"SCGUBUN~" = 'S' ) AND 
			( ~"PBPUR~".~"PUR101~".~"DIGUBUN~" = 'D' ) AND 
         ( ( ~"PBPUR~".~"PUR103~".~"VSRC~" = 'D' ) AND			
         ( ~"PBPUR~".~"PUR103~".~"DEPT~" = :ag_dept ) AND  
         ( ~"PBPUR~".~"PUR103~".~"ITNO~" = :ag_itno ) )    
	ORDER BY 1 ASC, 7,6 DESC " arguments=(("ag_dept", string),("ag_itno", string)) )
