 column=(type=decimal(2) updatewhereclause=yes name=movedanga dbname="MOVEDANGA" values=" 	0.00/" )
 column=(type=decimal(2) updatewhereclause=yes name=outdanga dbname="OUTDANGA" values=" 	0.00/" )
 column=(type=char(8) updatewhereclause=yes name=pedtm dbname="BOM001.PEDTM" )
 column=(type=char(8) updatewhereclause=yes name=pedte dbname="BOM001.PEDTE" )
 column=(type=char(15) updatewhereclause=yes name=optionno dbname="OPTIONNO" )
 column=(type=char(1) updatewhereclause=yes name=ygchk dbname="YGCHK" )
 retrieve="SELECT	F.PPITN as itemcode,  
			F.PWKCT,  F.PQTYM,  
			TRANSLATE(B.ITNM,' ','~"') AS ITEMNAME, TRANSLATE(B.SPEC,' ','~"') AS SPEC,
			C.PULS AS LOTSIZE,C.PULT AS LEADTIME,
			A.XUNIT,B.XUNIT as XUNIT1 ,B.CLS,B.SRCE,C.CONVQTY,B.COSTLS,B.COSTAV,C.ABCCD,
			Case	When 	B.SRCE NOT IN ( '05','03',' ' ) THEN
							Case	When 	C.CONVQTY <> 0 Then decimal(truncate(B.COSTLS / C.CONVQTY,2),8,2)  
									Else 	B.COSTLS End 
					Else 	0.00	End AS INPUTDANGA,
			Case	When 	B.SRCE NOT IN ( '05','03',' ' ) THEN
						Case 	When 	C.CONVQTY <> 0 Then decimal(truncate(CASE	WHEN	B.COSTAV	=	0	THEN	B.COSTLS ELSE B.COSTAV END / C.CONVQTY,2),8,2)   
								Else 	CASE	WHEN	B.COSTAV	=	0	THEN	B.COSTLS ELSE B.COSTAV END End	
					Else	0.00	End AS MOVEDANGA,
			Case	When 	B.SRCE NOT IN ( '05','03',' ' ) THEN
					Case	When B.OUTQTY <> 0 Then
								Case 	When 	C.CONVQTY <> 0 Then abs(decimal(truncate(decimal(truncate(( B.OUTAMT / C.OUTQTY),2),8,2) / C.CONVQTY,2),8,2))   
										Else 	abs(decimal(truncate(( B.OUTAMT / B.OUTQTY ),2),8,2)) End  
							Else 	Case	When 	C.CONVQTY <> 0 Then decimal(truncate(B.COSTLS / C.CONVQTY,2),8,2)  
									Else 	B.COSTLS End 	END 
					Else	0.00	End AS OUTDANGA,
			F.PEDTM,F.PEDTE, '               ' AS OptionNo,
			Case When trim(F.PEXDV) = '' then 
				(	select case when count(*) > 0 then 'Y' else 'N' end
				from pbpdm.bom001 d
				where d.pcmcd = f.pcmcd and d.plant = f.plant and
      			d.pdvsn = f.pdvsn and d.ppitn = f.pcitn and
      			d.pwkct = '8888' and
      			(( d.pedte = ' '  and d.pedtm <= :a_date ) or
      			( d.pedte <> ' ' and d.pedtm <= :a_date
                and d.pedte >= :a_date )) )
			Else
				(	select case when count(*) > 0 then 'Y' else 'N' end
				from pbpdm.bom001 d
				where d.pcmcd = f.pcmcd and d.plant = f.pexplant and
      			d.pdvsn = f.pdvsn and d.ppitn = f.pexdv and
      			d.pwkct = '8888' and
      			(( d.pedte = ' '  and d.pedtm <= :a_date ) or
      			( d.pedte <> ' ' and d.pedtm <= :a_date
                and d.pedte >= :a_date )) )
			End as ygchk
    FROM	~"PBPDM~".~"BOM001~" F,~"PBINV~".~"INV002~" A,~"PBINV~".~"INV402~" B ,~"PBINV~".~"INV101~" C	 
   WHERE F.PCMCD 	= '01' 		AND	F.PCMCD		= 	B.COMLTD	AND	B.COMLTD = 	A.COMLTD	AND	B.COMLTD = 	C.COMLTD						AND
			F.PPITN 	= 	A.ITNO	AND	A.ITNO	= 	B.ITNO	AND	B.ITNO	=  C.ITNO	AND	F.PLANT		=	B.XPLANT						AND
			F.PDVSN		= 	B.DIV		AND	C.XPLANT	=	B.XPLANT	AND 	C.DIV		= 	B.DIV		AND	B.XYEAR	=	SUBSTRING(:A_DATE,1,6)	AND
			F.PLANT 	= :A_PLANT 	AND 	F.PDVSN 	= 	:A_DIV  	AND	F.PCITN 	= 	:A_ITNO  AND  
         (( F.PEDTE =  ''  	and F.PEDTM <= :a_date )	or	( F.PEDTE <> ''	and F.PEDTM <= :a_date	and F.PEDTE >= :a_date ))
			
 
        


" arguments=(("a_plant", string),("a_div", string),("a_itno", string),("a_date", string))  sort="itemcode A " )
