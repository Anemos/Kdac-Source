 retrieve="  SELECT TSeqPart.AreaCode,   
         TSeqPart.DivisionCode,   
         TSeqPart.ItemCode,   
         TMSTITEM.ItemName,   
         TMSTITEM.ItemSpec,   
         TSeqPart.LotSize,   
         TSeqPart.SafeStockQty,   
         TMSTMODEL.ItemClass,   
         TMSTMODEL.ItemBuySource,   
         TMSTMODEL.AverageUnitCost,   
         TSeqPart.LastEmp,   
         TSeqPart.LastDate ,
         '1   ' as pt_chk   
    FROM TMSTITEM,   
         TMSTMODEL,   
         TSeqPart  
   WHERE ( TSeqPart.ItemCode = TMSTITEM.ItemCode ) and  
         ( TSeqPart.AreaCode = TMSTMODEL.AreaCode ) and  
         ( TSeqPart.DivisionCode = TMSTMODEL.DivisionCode ) and  
         ( TSeqPart.ItemCode = TMSTMODEL.ItemCode ) and  
         ( ( TSeqPart.AreaCode = :a_areacode ) AND  
         ( TSeqPart.DivisionCode = :a_divisioncode ) )    
" update="TSeqPart" updatewhere=0 updatekeyinplace=no arguments=(("a_areacode", string),("a_divisioncode", string))  sort="tseqpart_itemcode A " )
