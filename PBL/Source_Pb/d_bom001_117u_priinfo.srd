         ~"PBPDM~".~"BOM007~"  
   WHERE ( ~"PBINV~".~"INV101~".~"COMLTD~" = ~"PBPDM~".~"BOM007~".~"KRYCMCD~" ) and
			( ~"PBINV~".~"INV101~".~"XPLANT~" = ~"PBPDM~".~"BOM007~".~"KRYPLANT~" ) and  
         ( ~"PBINV~".~"INV101~".~"DIV~" = ~"PBPDM~".~"BOM007~".~"KRYDVSN~" ) and  
         ( ~"PBINV~".~"INV101~".~"ITNO~" = ~"PBPDM~".~"BOM007~".~"KRYITNO~" ) and  
         ( ( ~"PBPDM~".~"BOM007~".~"KRYCMCD~" = '01' ) AND
			( ~"PBPDM~".~"BOM007~".~"KRYPLANT~" = :a_plant ) AND 
			( ~"PBPDM~".~"BOM007~".~"KRYDVSN~" = :a_div ) )   
