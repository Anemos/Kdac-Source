 retrieve="  SELECT AA.~"RHFLAG~",   
         AA.~"RHPLANT~",   
         AA.~"RHDVSN~",   
         AA.~"RHITNO~",   
         AA.~"RHITNO1~",
			BB.~"PENAMEK~",
			AA.~"RHCMCD~",
			AA.~"RHINEMP~",
			AA.~"RHINCHK~",
			AA.~"RHINTIME~",
			AA.~"RHPLEMP~",
			AA.~"RHPLCHK~",
			AA.~"RHPLTIME~",
			AA.~"RHDLEMP~",
			AA.~"RHDLCHK~",
			AA.~"RHDLTIME~",
			AA.~"RHEDFM~",
			AA.~"RHEDTO~" 
    FROM ~"PBRTN~".~"RTN018~" AA LEFT OUTER JOIN ~"PBCOMMON~".~"DAC003~" BB
		ON AA.~"RHINEMP~" = BB.~"PEEMPNO~"
   WHERE ( AA.~"RHCMCD~" = :ag_cmcd) AND ( AA.~"RHPLANT~" = :ag_plant) AND ( AA.~"RHDVSN~" = :ag_dvsn) AND ( AA.~"RHITNO~" = :ag_itno) AND
			( AA.~"RHINEMP~" like :ag_inemp) AND ( AA.~"RHINCHK~" like :ag_inchk) AND ( AA.~"RHINTIME~" like :ag_intime) AND
			( AA.~"RHPLEMP~" like :ag_plemp) AND ( AA.~"RHPLCHK~" like :ag_plchk) AND ( AA.~"RHPLTIME~" like :ag_pltime) AND
			( AA.~"RHDLEMP~" like :ag_dlemp) AND ( AA.~"RHDLCHK~" like :ag_dlchk) AND ( AA.~"RHDLTIME~" like :ag_dltime)" arguments=(("ag_cmcd", string),("ag_plant", string),("ag_dvsn", string),("ag_itno", string),("ag_line1", string),("ag_line2", string),("ag_inemp", string),("ag_inchk", string),("ag_intime", string),("ag_plemp", string),("ag_plchk", string),("ag_pltime", string),("ag_dlemp", string),("ag_dlchk", string),("ag_dltime", string)) )
