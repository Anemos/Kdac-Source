 retrieve="  SELECT TMSTPARTKB.AreaCode,   
         TMSTPARTKB.DivisionCode,   
         TMSTPARTKB.SupplierCode,   
         TMSTPARTKB.ItemCode,   
         TMSTPARTKB.ApplyFrom,   
         TMSTPARTKB.PartType,   
         TMSTPARTKB.ChangeFlag,   
         TMSTPARTKB.PartModelID,   
         TMSTPARTKB.PartID,   
         TMSTPARTKB.UseCenterGubun,   
         TMSTPARTKB.UseCenter,   
         TMSTPARTKB.CostGubun,   
         TMSTPARTKB.RackQty,   
         TMSTPARTKB.RackCode,   
         TMSTPARTKB.StockGubun,   
         TMSTPARTKB.ReceiptLocation,   
         TMSTPARTKB.MailBoxNo,   
         TMSTPARTKB.SafetyStock,   
         TMSTPARTKB.UseFlag,   
         TMSTPARTKB.AutoReorderFlag,   
         TMSTPARTKB.KBUseFlag,   
         TMSTPARTKB.ChangeDate,   
         TMSTPARTKB.NormalKBSN,   
         TMSTPARTKB.TempKBSN,   
         TMSTPARTKB.PartKBPrintCount,   
         TMSTPARTKB.PartKBActiveCount,   
         TMSTPARTKB.PartKBPlanCount,
			TMSTPARTKB.BuyBackFlag,   
         TMSTPARTKB.LastEmp,   
         TMSTPARTKB.LastDate,   
         getdate() as c_sysdate,
			1 as inschk  
    FROM TMSTPARTKB  
   WHERE ( TMSTPARTKB.AreaCode = :as_areacode ) AND  
         ( TMSTPARTKB.DivisionCode = :as_divcode ) AND  
         ( TMSTPARTKB.SupplierCode = :as_suppcode ) AND  
         ( TMSTPARTKB.ItemCode = :as_itemcode )    
" update="TMSTPARTKB" updatewhere=1 updatekeyinplace=no arguments=(("as_areacode", string),("as_divcode", string),("as_suppcode", string),("as_itemcode", string)) )
