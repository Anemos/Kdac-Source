 retrieve="  SELECT ~"PBPDM~".~"BOM009~".~"FPDCD~", 
         ~"PBCOMMON~".~"DAC007~".~"PRNAME~" ,
			~"PBPDM~".~"BOM009~".~"FMDNO~",   
			~"PBINV~".~"INV002~".~"ITNM~",          
			~"PBPDM~".~"BOM009~".~"FDMCST~",   
         ~"PBPDM~".~"BOM009~".~"FDRCST~",   
         ~"PBPDM~".~"BOM009~".~"FDACST~",   
         ~"PBPDM~".~"BOM009~".~"FIMCST~",   
         ~"PBPDM~".~"BOM009~".~"FIRCST~",   
         ~"PBPDM~".~"BOM009~".~"FIACST~",   
         ~"PBPDM~".~"BOM009~".~"FDVOMT~",   
         ~"PBPDM~".~"BOM009~".~"FDVORT~",   
         ~"PBPDM~".~"BOM009~".~"FDVOAT~",   
         ~"PBPDM~".~"BOM009~".~"FTMCST~",   
         ~"PBPDM~".~"BOM009~".~"FTRCST~",   
         ~"PBPDM~".~"BOM009~".~"FTACST~",   
         ~"PBPDM~".~"BOM009~".~"FOSCST~",   
         ~"PBPDM~".~"BOM009~".~"FISRCE~"  
    FROM ~"PBPDM~".~"BOM009~",   
         ~"PBINV~".~"INV002~",~"PBCOMMON~".~"DAC007~"
   WHERE ( ~"PBPDM~".~"BOM009~".~"FMDNO~" = ~"PBINV~".~"INV002~".~"ITNO~" ) and  
         ( ~"PBPDM~".~"BOM009~".~"FDATE~" = :a_fdate ) AND  
			( ~"PBPDM~".~"BOM009~".~"FGUBUN~" = :a_comsec ) AND  
         ( ~"PBPDM~".~"BOM009~".~"FDVSN~" LIKE :a_fdiv ) AND  
         ( ~"PBPDM~".~"BOM009~".~"FPLANT~" LIKE :a_fplant ) AND
         ( ~"PBPDM~".~"BOM009~".~"FPDCD~" LIKE :a_fpdcd  )  AND
			( ~"PBPDM~".~"BOM009~".~"FPDCD~" = ~"PBCOMMON~".~"DAC007~".~"PRPRCD~" ) AND
         ( ~"PBPDM~".~"BOM009~".~"FCMCD~" = ~"PBCOMMON~".~"DAC007~".~"COMLTD~" )
" arguments=(("a_fplant", string),("a_comsec", string),("a_fdate", string),("a_fdiv", string),("a_fpdcd", string))  sort="bom009_fpdcd A bom009_fmdno A " )
