 retrieve="SELECT '전체' AS COFLNAME ,
       '전체' AS COITNAME ,
       '' AS COCODE,
       '' cocode2 
FROM ~"PBCOMMON~".~"DAC002~"  
WHERE 1=1
UNION 
SELECT ~"PBCOMMON~".~"DAC002~".~"COFLNAME~",   
       ~"PBCOMMON~".~"DAC002~".~"COITNAME~",   
       ~"PBCOMMON~".~"DAC002~".~"COCODE~",   
       {fn substring(~"PBCOMMON~".~"DAC002~".~"COCODE~",1,2)} as cocode2  
FROM ~"PBCOMMON~".~"DAC002~"  
WHERE ~"PBCOMMON~".~"DAC002~".~"COMLTD~" = '01'
  AND ~"PBCOMMON~".~"DAC002~".~"COGUBUN~" = :a_cogubun   
ORDER BY 4" arguments=(("a_cogubun", string)) )
