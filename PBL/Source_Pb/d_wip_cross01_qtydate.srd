         ~"PBINV~".~"INV401~".~"TQTY4~",   
         ~"PBWIP~".~"WIP004~".~"WDDATE~",   
         ~"PBWIP~".~"WIP004~".~"WDPRQT~"  
    FROM ~"PBINV~".~"INV401~",   
