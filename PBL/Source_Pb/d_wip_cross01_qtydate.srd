   WHERE ( ~"PBINV~".~"INV401~".~"COMLTD~" = ~"PBWIP~".~"WIP004~".~"WDCMCD~" ) and  
         ( ~"PBINV~".~"INV401~".~"SLIPTYPE~" = ~"PBWIP~".~"WIP004~".~"WDPRSRTY~" ) and  
         ( ~"PBINV~".~"INV401~".~"SRNO~" = ~"PBWIP~".~"WIP004~".~"WDPRSRNO~" ) and
			( ~"PBINV~".~"INV401~".~"SRNO1~" = ~"PBWIP~".~"WIP004~".~"WDPRSRNO1~" ) and
			( ~"PBINV~".~"INV401~".~"SRNO2~" = ~"PBWIP~".~"WIP004~".~"WDPRSRNO2~" ) and  
         ( ~"PBINV~".~"INV401~".~"XPLANT~" = ~"PBWIP~".~"WIP004~".~"WDPLANT~" ) and  
         ( ~"PBINV~".~"INV401~".~"DIV~" = ~"PBWIP~".~"WIP004~".~"WDDVSN~" ) and  
         ( ( ~"PBINV~".~"INV401~".~"COMLTD~" = :a_cmcd ) AND  
         ( ( ( ~"PBINV~".~"INV401~".~"TDTE4~" >= :a_fromdt ) AND 
			    ( ~"PBINV~".~"INV401~".~"TDTE4~" <= :a_todt ) ) OR
			  ( ( ~"PBWIP~".~"WIP004~".~"WDDATE~" >= :a_fromdt ) AND 
			    ( ~"PBWIP~".~"WIP004~".~"WDDATE~" <= :a_todt ) ) ) AND 
			(( ~"PBINV~".~"INV401~".~"TDTE4~" <> ~"PBWIP~".~"WIP004~".~"WDDATE~" ) OR 
         ( ~"PBINV~".~"INV401~".~"TQTY4~" <> ~"PBWIP~".~"WIP004~".~"WDPRQT~" )) )  
" arguments=(("a_cmcd", string),("a_fromdt", string),("a_todt", string),("a_yyyymm", string)) )
