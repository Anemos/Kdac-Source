 retrieve="  SELECT ~"PBRTN~".~"RTN015~".~"REPLANT~",   
         ~"PBRTN~".~"RTN015~".~"REDVSN~",   
         ~"PBRTN~".~"RTN015~".~"REITNO~",   
         ~"PBRTN~".~"RTN015~".~"RELINE1~",   
         ~"PBRTN~".~"RTN015~".~"RELINE2~",   
         ~"PBRTN~".~"RTN015~".~"REOPSQ~",   
         ~"PBRTN~".~"RTN015~".~"REOPNO~",   
         ~"PBRTN~".~"RTN015~".~"REOPNM~",   
         ~"PBRTN~".~"RTN015~".~"RELINE3~",   
         ~"PBRTN~".~"RTN015~".~"REBMTM~",   
         ~"PBRTN~".~"RTN015~".~"REBLTM~",   
         ~"PBRTN~".~"RTN015~".~"REBSTM~",
			~"PBRTN~".~"RTN015~".~"RELBCNT~"     
FROM ~"PBRTN~".~"RTN015~"  
    WHERE ( ~"PBRTN~".~"RTN015~".~"RECMCD~"  = :a_cmcd ) AND  
          ( ~"PBRTN~".~"RTN015~".~"REPLANT~" = :a_plant ) AND   
         ( ~"PBRTN~".~"RTN015~".~"REDVSN~"   = :a_div ) AND  
         ( ~"PBRTN~".~"RTN015~".~"REITNO~"   = :a_itno ) AND 
         ( ~"PBRTN~".~"RTN015~".~"RELINE1~" = :a_line1 AND ~"PBRTN~".~"RTN015~".~"RELINE2~" = :a_line2 ) AND
         (~"PBRTN~".~"RTN015~".~"REEDFM~" <= :a_ajdate AND ~"PBRTN~".~"RTN015~".~"REEDTO~" >= :a_ajdate AND
          ~"PBRTN~".~"RTN015~".~"REEDFM~" <= ~"PBRTN~".~"RTN015~".~"REEDTO~" )
" arguments=(("a_cmcd", string),("a_plant", string),("a_div", string),("a_itno", string),("a_line1", string),("a_line2", string),("a_ajdate", string))  sort="replant A redvsn A reitno A reline1 A reline2 A reopsq A reopno A " )
