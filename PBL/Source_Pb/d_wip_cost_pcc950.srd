 column=(type=decimal(0) updatewhereclause=yes name=bgat dbname="BGAT" )
 retrieve="SELECT TMP.CMCD AS COMLTD,
	TMP.PLANT AS XPLANT,
	TMP.DVSN AS DIV,
	TMP.PDCD AS PDCD,
	SUM(TMP.BGAT) AS BGAT
FROM ( SELECT ~"PBWIP~".~"WIP002~".~"WBCMCD~" AS CMCD,
       ~"PBWIP~".~"WIP002~".~"WBPLANT~" AS PLANT,   
       ~"PBWIP~".~"WIP002~".~"WBDVSN~" AS DVSN,   
       SUBSTR(~"PBINV~".~"INV402~".~"PDCD~",1,2) AS PDCD,    
       sum(~"PBWIP~".~"WIP002~".~"WBBGAT1~") AS BGAT
  FROM ~"PBWIP~".~"WIP002~",   
       ~"PBINV~".~"INV402~"
 WHERE ( ~"PBWIP~".~"WIP002~".~"WBCMCD~" = ~"PBINV~".~"INV402~".~"COMLTD~" ) and  
       ( ~"PBWIP~".~"WIP002~".~"WBPLANT~" = ~"PBINV~".~"INV402~".~"XPLANT~" ) and  
       ( ~"PBWIP~".~"WIP002~".~"WBDVSN~" = ~"PBINV~".~"INV402~".~"DIV~" ) and  
       ( ~"PBWIP~".~"WIP002~".~"WBITNO~" = ~"PBINV~".~"INV402~".~"ITNO~" ) and 
		 ( ~"PBINV~".~"INV402~".~"XYEAR~" = :ag_refdate) and 
       ( ( ~"PBWIP~".~"WIP002~".~"WBCMCD~" = :ag_cmcd ) AND  
       ( ~"PBWIP~".~"WIP002~".~"WBPLANT~" = :ag_plant ) AND  
       ( ~"PBWIP~".~"WIP002~".~"WBDVSN~" = :ag_dvsn ) AND
		 ( ~"PBWIP~".~"WIP002~".~"WBIOCD~" IN ('1','2') ) AND  
       ( (~"PBWIP~".~"WIP002~".~"WBYEAR~"||~"PBWIP~".~"WIP002~".~"WBMONTH~" = :ag_date )) ) and
        (~"PBWIP~".~"WIP002~".~"WBBGAT1~" <> 0 or ~"PBWIP~".~"WIP002~".~"WBINAT1~" <> 0 or ~"PBWIP~".~"WIP002~".~"WBINAT2~" <> 0 or ~"PBWIP~".~"WIP002~".~"WBINAT3~" <> 0 or
         ~"PBWIP~".~"WIP002~".~"WBUSAT1~" <> 0 or ~"PBWIP~".~"WIP002~".~"WBUSAT2~" <> 0 or ~"PBWIP~".~"WIP002~".~"WBUSAT3~" <> 0 or
         ~"PBWIP~".~"WIP002~".~"WBUSAT4~" <> 0 or ~"PBWIP~".~"WIP002~".~"WBUSAT5~" <> 0 or ~"PBWIP~".~"WIP002~".~"WBUSAT6~" <> 0 or 
         ~"PBWIP~".~"WIP002~".~"WBUSAT7~" <> 0 or ~"PBWIP~".~"WIP002~".~"WBUSAT8~" <> 0 or ~"PBWIP~".~"WIP002~".~"WBUSAT9~" <> 0 or 
         ~"PBWIP~".~"WIP002~".~"WBUSATA~" <> 0)    
GROUP BY ~"PBWIP~".~"WIP002~".~"WBCMCD~",   
         ~"PBWIP~".~"WIP002~".~"WBPLANT~",   
         ~"PBWIP~".~"WIP002~".~"WBDVSN~",   
         SUBSTR(~"PBINV~".~"INV402~".~"PDCD~",1,2)
UNION ALL
SELECT ~"PBWIP~".~"WIP003~".~"WCCMCD~" AS CMCD,
         ~"PBWIP~".~"WIP003~".~"WCPLANT~" AS PLANT,   
         ~"PBWIP~".~"WIP003~".~"WCDVSN~" AS DVSN,   
         SUBSTR(~"PBINV~".~"INV402~".~"PDCD~",1,2) AS PDCD,  
         sum(~"PBWIP~".~"WIP003~".~"WCBGAT1~") AS BGAT
    FROM ~"PBWIP~".~"WIP003~",   
         ~"PBINV~".~"INV402~"  
   WHERE ( ~"PBWIP~".~"WIP003~".~"WCCMCD~" = ~"PBINV~".~"INV402~".~"COMLTD~" ) and  
         ( ~"PBWIP~".~"WIP003~".~"WCPLANT~" = ~"PBINV~".~"INV402~".~"XPLANT~" ) and  
         ( ~"PBWIP~".~"WIP003~".~"WCDVSN~" = ~"PBINV~".~"INV402~".~"DIV~" ) and  
         ( ~"PBWIP~".~"WIP003~".~"WCITNO~" = ~"PBINV~".~"INV402~".~"ITNO~" ) and 
			( ~"PBINV~".~"INV402~".~"XYEAR~" = :ag_refdate ) and  
         ( ( ~"PBWIP~".~"WIP003~".~"WCCMCD~" = :ag_cmcd ) AND  
         ( ~"PBWIP~".~"WIP003~".~"WCPLANT~" = :ag_plant ) AND  
         ( ~"PBWIP~".~"WIP003~".~"WCDVSN~" = :ag_dvsn ) AND  
         ( (~"PBWIP~".~"WIP003~".~"WCYEAR~"||~"PBWIP~".~"WIP003~".~"WCMONTH~" = :ag_date )) )  and
         (~"PBWIP~".~"WIP003~".~"WCBGAT1~" <> 0 or ~"PBWIP~".~"WIP003~".~"WCINAT1~" <> 0 or ~"PBWIP~".~"WIP003~".~"WCUSAT1~" <> 0 or
          ~"PBWIP~".~"WIP003~".~"WCUSAT2~" <> 0 or ~"PBWIP~".~"WIP003~".~"WCUSAT3~" <> 0 or ~"PBWIP~".~"WIP003~".~"WCUSAT4~" <> 0 or
          ~"PBWIP~".~"WIP003~".~"WCUSAT5~" <> 0 or ~"PBWIP~".~"WIP003~".~"WCUSAT6~" <> 0 or ~"PBWIP~".~"WIP003~".~"WCUSAT7~" <> 0 or
          ~"PBWIP~".~"WIP003~".~"WCUSAT8~" <> 0 or ~"PBWIP~".~"WIP003~".~"WCUSAT9~" <> 0 )  
GROUP BY ~"PBWIP~".~"WIP003~".~"WCCMCD~",   
         ~"PBWIP~".~"WIP003~".~"WCPLANT~",   
         ~"PBWIP~".~"WIP003~".~"WCDVSN~",   
         SUBSTR(~"PBINV~".~"INV402~".~"PDCD~",1,2)
	) TMP
GROUP BY TMP.CMCD, TMP.PLANT, TMP.DVSN, TMP.PDCD" arguments=(("ag_cmcd", string),("ag_plant", string),("ag_dvsn", string),("ag_date", string),("ag_refdate", string)) )
