 retrieve="  SELECT A.AREACODE			,
         A.DIVISIONCODE		,
         A.SUPPLIERCODE		,
         D.SUPPLIERKORNAME	,
         A.DELIVERYNO		,
         A.ITEMCODE			,
         E.ITEMNAME			,
         A.MAKEDATE			,
         A.CHARGENAME		,
         A.STANDARDREVNO	,
         A.SUPPLIERLOTQTY	,
         A.KBCOUNT			,
         A.SUPPLIERDELIVERYQTY	,
         A.QCMETHOD			,
         A.REMARK				,
         A.KDACREMARK		,
         A.JUDGEFLAG			,
         A.GOODQTY			,
         A.BADQTY				,
         A.BADREASON			,
         A.BADMEMO			,
         A.INSPECTORCODE	,
         F.EMPNAME 		AS AS_INSPECTOREMPNAME		,
         A.CONFIRMCODE		,
         G.EMPNAME		AS AS_CONFIRMEMPNAME		,
         A.CONFIRMFLAG		,
         A.PRINTFLAG			,
         A.QCDATE				,
         A.QCKBFLAG			,
         A.SLNO				,
         A.DELIVERYDATE		,
         A.DELIVERYTIME		,
			AS_SUPPLIERLOTNO  ,
			CASE A.QCMETHOD WHEN '1' THEN '전수검사' ELSE '발췌검사' END AS QCMETHOD_NM,
			CASE A.JUDGEFLAG WHEN '1' THEN '합격' WHEN '2' THEN '불합격' WHEN '3' THEN '선별' ELSE '미판정' END AS JUDGEFLAG_NM
    FROM TQQCRESULT		A,
         TMSTSUPPLIER	D,
         TMSTITEM			E,
         TMSTEMP			F,
         TMSTEMP			G,
			( SELECT TOP 1 Y.SUPPLIERLOTNO AS AS_SUPPLIERLOTNO 
				 FROM TQQCRESULT X,
						TQQCRESULTDETAIL Y
				WHERE X.AREACODE		*= Y.AREACODE
				  AND X.DIVISIONCODE	*= Y.DIVISIONCODE	
				  AND X.SUPPLIERCODE	*= Y.SUPPLIERCODE
				  AND X.DELIVERYNO	*= Y.DELIVERYNO
				  AND X.ITEMCODE		*= Y.ITEMCODE
				  AND X.AREACODE		 = :as_areacode
				  AND X.DIVISIONCODE	 = :as_divisioncode
				  AND X.SUPPLIERCODE	 = :as_suppliercode
				  AND X.DELIVERYNO	 = :as_deliveryno
				  AND X.ITEMCODE		 = :as_itemcode 
				  AND X.STANDARDREVNO = :as_revno ) TMP
   WHERE A.SUPPLIERCODE*= D.SUPPLIERCODE
	 AND A.ITEMCODE	  *= E.ITEMCODE
	 AND A.INSPECTORCODE*= F.EMPNO
	 AND A.CONFIRMCODE  *= G.EMPNO
	 AND A.AREACODE		= :as_areacode
	 AND A.DIVISIONCODE	= :as_divisioncode
	 AND A.SUPPLIERCODE	= :as_suppliercode
	 AND A.DELIVERYNO		= :as_deliveryno
	 AND A.ITEMCODE		= :as_itemcode
	 AND A.STANDARDREVNO = :as_revno
  " arguments=(("as_areacode", string),("as_divisioncode", string),("as_suppliercode", string),("as_deliveryno", string),("as_itemcode", string),("as_revno", string)) )
