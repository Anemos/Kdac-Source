         ( ( ~"PBINV~".~"INV101~".~"ITNO~" like :ag_itno ) AND  
         ( ~"PBINV~".~"INV002~".~"ITNM~" like :ag_itno ) AND  
         ( ~"PBINV~".~"INV101~".~"MASS~" = :ag_mass ) )    
