         ~"PBINV~".~"INV101~".~"XUNIT~",   
         ~"PBINV~".~"INV101~".~"MASS~",   
         ~"PBINV~".~"INV101~".~"COMCD~",   
         ~"PBINV~".~"INV101~".~"MLAN~"  
    FROM ~"PBINV~".~"INV002~",   
         ~"PBINV~".~"INV101~"  
   WHERE ( ~"PBINV~".~"INV002~".~"COMLTD~" = ~"PBINV~".~"INV101~".~"COMLTD~" ) and  
         ( ~"PBINV~".~"INV002~".~"ITNO~" = ~"PBINV~".~"INV101~".~"ITNO~" ) and  
