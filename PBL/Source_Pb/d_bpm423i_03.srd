         ~"PBBPM~".~"BPM517~".~"WVDCSTE~",   
         ~"PBBPM~".~"BPM517~".~"WSLPNOD~",   
         ~"PBBPM~".~"BPM517~".~"WSLPNOE~",   
         ~"PBBPM~".~"BPM517~".~"WCOSTCD~",
			~"PBBPM~".~"BPM517~".~"WDSHEET~",   
         ~"PBBPM~".~"BPM517~".~"WXCAUSE~",   
         ~"PBBPM~".~"BPM517~".~"WADJDT~" ,   
         ~"PBBPM~".~"BPM517~".~"WCOSTOLD~",   
         ~"PBBPM~".~"BPM517~".~"WCOSTD0A~",
			~"PBBPM~".~"BPM517~".~"WYGCST~",
			~"PBBPM~".~"BPM517~".~"WYCSTORG~",
			~"PBBPM~".~"BPM517~".~"WYGCST1~",
			~"PBBPM~".~"BPM517~".~"WMRATE~",
			SUM(~"PBBPM~".~"BPM517~".~"WQTY1~") AS WQTY1,   
         SUM(~"PBBPM~".~"BPM517~".~"WQTY2~") AS WQTY2,   
         SUM(~"PBBPM~".~"BPM517~".~"WQTY3~") AS WQTY3,   
         SUM(~"PBBPM~".~"BPM517~".~"WQTY4~") AS WQTY4,   
         SUM(~"PBBPM~".~"BPM517~".~"WQTY5~") AS WQTY5,   
         SUM(~"PBBPM~".~"BPM517~".~"WQTY6~") AS WQTY6,   
         SUM(~"PBBPM~".~"BPM517~".~"WQTY7~") AS WQTY7,   
         SUM(~"PBBPM~".~"BPM517~".~"WQTY8~") AS WQTY8,   
         SUM(~"PBBPM~".~"BPM517~".~"WQTY9~") AS WQTY9,   
         SUM(~"PBBPM~".~"BPM517~".~"WQTY10~") AS WQTY10,   
         SUM(~"PBBPM~".~"BPM517~".~"WQTY11~") AS WQTY11,   
         SUM(~"PBBPM~".~"BPM517~".~"WQTY12~") AS WQTY12                  
    FROM ~"PBBPM~".~"BPM517~"  
   WHERE ( ~"PBBPM~".~"BPM517~".~"COMLTD~" = '01' ) AND 
			( ~"PBBPM~".~"BPM517~".~"XYEAR~" = :a_year ) AND  
         ( ~"PBBPM~".~"BPM517~".~"WREV~"  = :a_rev ) AND  
         ( ~"PBBPM~".~"BPM517~".~"WGUBUN~"  = :a_gubun ) AND
         ( ~"PBBPM~".~"BPM517~".~"WSRCE~" <> '03' ) AND 
         ( ~"PBBPM~".~"BPM517~".~"WPLANT~" like :a_xplant ) AND  
         ( ~"PBBPM~".~"BPM517~".~"WDVSN~"  like :a_div ) AND  
         ( ~"PBBPM~".~"BPM517~".~"WPDCD~"  like :a_pdcd ) AND  
         ( ~"PBBPM~".~"BPM517~".~"WITNO~"  like :a_itno ) 
GROUP BY ~"PBBPM~".~"BPM517~".~"WITNO~",    
         TRANSLATE(~"PBBPM~".~"BPM517~".~"WITEM~",' ','~"'),   
