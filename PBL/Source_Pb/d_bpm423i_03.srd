			~"PBBPM~".~"BPM517~".~"WDSHEET~",   
         ~"PBBPM~".~"BPM517~".~"WXCAUSE~",   
         ~"PBBPM~".~"BPM517~".~"WADJDT~" ,   
         ~"PBBPM~".~"BPM517~".~"WCOSTOLD~",   
         ~"PBBPM~".~"BPM517~".~"WCOSTD0A~",
