         ~"PBBPM~".~"BPM517~".~"WVDNO~",   
         ~"PBBPM~".~"BPM517~".~"WCURR~",   
         ~"PBBPM~".~"BPM517~".~"WCOST~",   
         ~"PBBPM~".~"BPM517~".~"WCOSTW~",   
