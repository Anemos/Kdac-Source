         TRANSLATE(~"PBBPM~".~"BPM517~".~"WITEM~",' ','~"') AS WITEM,   
         TRANSLATE(~"PBBPM~".~"BPM517~".~"WSPEC~",' ','~"') AS WSPEC,   
