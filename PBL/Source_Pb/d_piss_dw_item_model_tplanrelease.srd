 retrieve="  Select	DISTINCT 
   F.DIVISIONNAME,
   C.PRODUCTGROUPNAME,  
  	A.ItemCode,	
	B.ItemName	
  From	TMSTMODEL		A,
   TMSTITEM		B,
	TMSTPRODUCTGROUP	C,
	TMSTMODELGROUP	D,
	TMSTAREA		E,
	TMSTDIVISION		F,
   TPLANRELEASE                 G 
 Where	G.AreaCode                       = :ps_areacode               And
         G.DivisionCode                  = :ps_divisioncode           And
         G.PlanDate                     like  :ps_yymm + '%'             And
       	A.ProductGroup	          like  :ps_productgroup         And
			A.ModelGroup	          like  :ps_modelgroup         And
         G.AreaCode		= C.AreaCode		And
	G.AreaCode		= D.AreaCode		And
	G.AreaCode		= E.AreaCode		And
	G.AreaCode		= F.AreaCode		And
   G.AreaCode		= A.AreaCode		And
	G.DivisionCode		= C.DivisionCode		And
	G.DivisionCode		= D.DivisionCode		And
	G.DivisionCode		= F.DivisionCode		And
	G.DivisionCode		= A.DivisionCode		And
	G.ItemCode		= B.ItemCode		And
	G.ItemCode		= A.ItemCode		And
	A.ProductGroup		= C.ProductGroup	And
	A.ProductGroup		= D.ProductGroup	And
	A.ModelGroup		= D.ModelGroup		And
	F.DisplayFlag		= 'Y'
 UNION ALL
   Select TOP 1	' ' , ' ' ,' ALL','ALL'
  From	SYSUSERS 

