 retrieve="  SELECT ~"PBBPM~".~"BPM504~".~"PCITN~",   
         ~"PBBPM~".~"BPM504~".~"PQTYM~",   
         ~"PBBPM~".~"BPM504~".~"PWKCT~",   
         ~"PBBPM~".~"BPM504~".~"PEDTM~",   
         ~"PBBPM~".~"BPM504~".~"PEDTE~",   
         ~"PBBPM~".~"BPM504~".~"PDVSN~",   
         ~"PBBPM~".~"BPM504~".~"PPITN~",   
         ~"PBBPM~".~"BPM504~".~"PROUT~",   
         ~"PBBPM~".~"BPM504~".~"PCHDT~",   
         ~"PBBPM~".~"BPM504~".~"PQTYE~",   
         ~"PBBPM~".~"BPM504~".~"POPCD~",   
         ~"PBBPM~".~"BPM504~".~"PEXDV~",   
         ~"PBBPM~".~"BPM504~".~"PCHCD~",   
         ~"PBBPM~".~"BPM504~".~"POSCD~",   
         ~"PBBPM~".~"BPM504~".~"PEBST~",   
         ~"PBBPM~".~"BPM504~".~"PINDT~",   
         ~"PBBPM~".~"BPM504~".~"PEMNO~",   ~"PBBPM~".~"BPM504~".~"PEXPLANT~",
         ' ' as cp_chk  
    FROM ~"PBBPM~".~"BPM504~"  
   WHERE ( ~"PBBPM~".~"BPM504~".~"PCMCD~" = '01' ) AND
			( ~"PBBPM~".~"BPM504~".~"XYEAR~" = :a_year ) AND
			( ~"PBBPM~".~"BPM504~".~"REVNO~" = :a_revno ) AND
			( ~"PBBPM~".~"BPM504~".~"PLANT~" = :a_plant ) AND
         ( ~"PBBPM~".~"BPM504~".~"PDVSN~" = :a_div ) AND  
         ( ~"PBBPM~".~"BPM504~".~"PPITN~" = :a_itno ) AND  
         ( (~"PBBPM~".~"BPM504~".~"PEDTM~" <= ~"PBBPM~".~"BPM504~".~"PEDTE~" AND ~"PBBPM~".~"BPM504~".~"PEDTE~" <> ' ' AND ~"PBBPM~".~"BPM504~".~"PEDTE~" >= :a_date )
          OR  
         ( ~"PBBPM~".~"BPM504~".~"PEDTE~" = ' ' ))
         " arguments=(("a_year", string),("a_revno", string),("a_plant", string),("a_div", string),("a_itno", string),("a_date", string)) )
