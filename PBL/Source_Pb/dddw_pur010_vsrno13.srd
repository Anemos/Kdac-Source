 retrieve="SELECT 1, ''AS NAMECODE ,'' AS CONAME , '' AS CODE, ' ' as temp
FROM PBPUR.PURDUM 
UNION
SELECT 2,MAX(TRIM(B.VNDNM) || '  [ ' ||B.VSRNO)||' ]' AS NAMECODE,
       MAX(B.VNDNM) AS CONAME,            
		 B.VSRNO AS CODE    , ' ' as temp     
FROM PBPUR.PUR102 A , PBPUR.PUR101 B
WHERE A.COMLTD = B.COMLTD
  AND A.VSRNO = B.VSRNO
  AND B.SCGUBUN = 'S'
  AND B.DIGUBUN = 'D'
  AND A.COMLTD = '01'
  AND A.DEPT IN ('D','Y')
  AND A.XSTOP = 'O'
GROUP BY B.VSRNO
ORDER BY  1,3 " )
