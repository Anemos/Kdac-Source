 column=(type=char(64) updatewhereclause=yes name=namecode dbname="NAMECODE" )
 column=(type=char(50) updatewhereclause=yes name=coname dbname="CONAME" )
 column=(type=char(8) updatewhereclause=yes name=code dbname="CODE" )
