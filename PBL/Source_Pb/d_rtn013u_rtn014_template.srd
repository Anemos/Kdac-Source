 retrieve="  SELECT A.~"RDPLANT~",   
         A.~"RDDVSN~",   
         A.~"RDITNO~",   
         A.~"RDLINE1~",   
         A.~"RDLINE2~",   
         A.~"RDOPNO~",   
         A.~"RDREMK~",   
         A.~"RDNVMO~",   
         A.~"RDMCNO~",   
         A.~"RDTERM~",   
         A.~"RDMCTM~",   
         A.~"RDLBTM~",   
         A.~"RDLOGID~",
			B.~"RCOPSQ~"  
    FROM ~"PBRTN~".~"RTN014~" A INNER JOIN ~"PBRTN~".~"RTN013~" B
		ON A.~"RDCMCD~" = B.~"RCCMCD~" AND
			A.~"RDPLANT~" = B.~"RCPLANT~" AND   
         A.~"RDDVSN~" = B.~"RCDVSN~" AND   
         A.~"RDITNO~" = B.~"RCITNO~" AND   
         A.~"RDLINE1~" = B.~"RCLINE1~" AND  
         A.~"RDLINE2~" = B.~"RCLINE2~" AND   
         A.~"RDOPNO~" = B.~"RCOPNO~"  
   WHERE ( A.~"RDCMCD~" = :ag_cmcd ) AND  
         ( A.~"RDPLANT~" = :ag_plant ) AND  
         ( A.~"RDDVSN~" = :ag_dvsn ) AND  
         ( A.~"RDITNO~" = :ag_itno ) AND  
         ( A.~"RDLINE1~" = :ag_line1 ) AND  
         ( A.~"RDLINE2~" = :ag_line2 )    
" arguments=(("ag_cmcd", string),("ag_plant", string),("ag_dvsn", string),("ag_itno", string),("ag_line1", string),("ag_line2", string)) )
