 retrieve="  SELECT A.AREACODE,   
         A.DIVISIONCODE,   
         A.PRDENDDATE,   
         A.KBNO,   
         A.PRDENDTIME,   
         A.ITEMCODE,   
         B.ITEMNAME,   
         A.PRODUCTGROUP,   
         A.MODELGROUP,   
         A.RACKQTY,   
         A.QCQTY,   
         A.QCFLAG,   
         A.QCEMPNO,   
         A.QCDATE  
    FROM TQCONTAINQCENTRY  A,
         TMSTITEM          B
   WHERE A.ITEMCODE     = B.ITEMCODE
     AND A.AREACODE		= :as_areacode
     AND A.DIVISIONCODE = :as_divisioncode
     AND A.PRODUCTGROUP LIKE :as_product
     AND A.MODELGROUP   LIKE :as_model
     AND A.QCFLAG			<> '0'
 " update="TQCONTAINQCENTRY" updatewhere=1 updatekeyinplace=no arguments=(("as_areacode", string),("as_divisioncode", string),("as_product", string),("as_model", string)) )
