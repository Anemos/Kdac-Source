FROM ~"PBPDM~".~"BOM001~",~"PBINV~".~"INV002~",~"PBINV~".~"INV101~"  
WHERE 
		( ~"PBPDM~".~"BOM001~".~"PCMCD~" 	= '01' ) 				AND
