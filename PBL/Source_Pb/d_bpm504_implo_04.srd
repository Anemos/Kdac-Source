 retrieve="  SELECT ~"PBBPM~".~"BPM504~".~"PPITN~",   
         ~"PBBPM~".~"BPM504~".~"PQTYM~",   
         ~"PBBPM~".~"BPM504~".~"PWKCT~",   
         ~"PBBPM~".~"BPM504~".~"PEDTM~",   
         ~"PBBPM~".~"BPM504~".~"PEDTE~",   
         ~"PBBPM~".~"BPM504~".~"PDVSN~",
			~"PBBPM~".~"BPM504~".~"PLANT~",   
         ~"PBBPM~".~"BPM504~".~"PROUT~",   
         ~"PBBPM~".~"BPM504~".~"PCHDT~",   
         ~"PBBPM~".~"BPM504~".~"PCITN~",   
         ~"PBBPM~".~"BPM504~".~"PQTYE~",   
         ~"PBBPM~".~"BPM504~".~"POPCD~",
			~"PBBPM~".~"BPM504~".~"PEXPLANT~",   
         ~"PBBPM~".~"BPM504~".~"PEXDV~",   
         ~"PBBPM~".~"BPM504~".~"PCHCD~",   
         ~"PBBPM~".~"BPM504~".~"PINDT~",   
         ~"PBBPM~".~"BPM504~".~"PEMNO~",   
         ~"PBBPM~".~"BPM504~".~"POSCD~",   
         ~"PBBPM~".~"BPM504~".~"PEBST~",
         ~"PBBPM~".~"BPM503~".~"CLS~",
         ~"PBBPM~".~"BPM503~".~"SRCE~",
         ~"PBBPM~".~"BPM502~".~"ITNM~",
         ~"PBBPM~".~"BPM502~".~"XUNIT~",
         '               ' AS OPT_ITNO   
    FROM ~"PBBPM~".~"BPM504~" INNER JOIN ~"PBBPM~".~"BPM502~"
		ON ( ~"PBBPM~".~"BPM504~".~"XYEAR~" = ~"PBBPM~".~"BPM502~".~"XYEAR~" ) AND
			( ~"PBBPM~".~"BPM504~".~"REVNO~" = ~"PBBPM~".~"BPM502~".~"REVNO~" ) AND
			( ~"PBBPM~".~"BPM504~".~"PPITN~" = ~"PBBPM~".~"BPM502~".~"ITNO~" ) 
		INNER JOIN ~"PBBPM~".~"BPM503~"
		ON ( ~"PBBPM~".~"BPM504~".~"XYEAR~" = ~"PBBPM~".~"BPM503~".~"XYEAR~" ) AND
			( ~"PBBPM~".~"BPM504~".~"REVNO~" = ~"PBBPM~".~"BPM503~".~"REVNO~" ) AND
			( ~"PBBPM~".~"BPM504~".~"PLANT~" = ~"PBBPM~".~"BPM503~".~"XPLANT~" ) AND  
         ( ~"PBBPM~".~"BPM504~".~"PDVSN~" = ~"PBBPM~".~"BPM503~".~"DIV~" ) AND  
         ( ~"PBBPM~".~"BPM504~".~"PPITN~" = ~"PBBPM~".~"BPM503~".~"ITNO~" )
   WHERE ( ~"PBBPM~".~"BPM504~".~"PCMCD~" = '01' ) AND
			( ~"PBBPM~".~"BPM504~".~"XYEAR~" = :a_year ) AND
			( ~"PBBPM~".~"BPM504~".~"REVNO~" = :a_revno ) AND
			( ~"PBBPM~".~"BPM504~".~"PLANT~" = :a_plant ) AND
         ( ~"PBBPM~".~"BPM504~".~"PDVSN~" = :a_div ) AND  
         ( ~"PBBPM~".~"BPM504~".~"PCITN~" = :a_itno ) AND  
         ( (~"PBBPM~".~"BPM504~".~"PEDTM~" <= ~"PBBPM~".~"BPM504~".~"PEDTE~" AND ~"PBBPM~".~"BPM504~".~"PEDTE~" <> ' ' AND ~"PBBPM~".~"BPM504~".~"PEDTE~" >= :a_date )
          OR  
         ( ~"PBBPM~".~"BPM504~".~"PEDTE~" = ' ' ))
         
ORDER BY ~"PBBPM~".~"BPM504~".~"PPITN~" ASC,   
         ~"PBBPM~".~"BPM504~".~"PEDTM~" ASC   
" arguments=(("a_year", string),("a_revno", string),("a_plant", string),("a_div", string),("a_itno", string),("A_DATE", string)) )
