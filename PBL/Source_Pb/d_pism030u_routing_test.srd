    FROM ~"PBRTN~".~"RTN003CPY~"  
    WHERE ( ~"PBRTN~".~"RTN003CPY~".~"RCCMCD~"  = :a_cmcd ) AND  
          ( ~"PBRTN~".~"RTN003CPY~".~"RCPLANT~" = :a_plant ) AND   
