table(column=(type=char(15) updatewhereclause=yes name=BPM001_pcitn dbname="BPM001.PCITN" )
 column=(type=char(15) updatewhereclause=yes name=BPM001_ppitn dbname="BPM001.PPITN" )
 column=(type=char(8) updatewhereclause=yes name=BPM001_pedtm dbname="BPM001.PEDTM" )
 column=(type=char(8) updatewhereclause=yes name=BPM001_pedte dbname="BPM001.PEDTE" )
 retrieve="  SELECT ~"PBBPM~".~"BPM001~".~"PCITN~",   
         ~"PBBPM~".~"BPM001~".~"PPITN~",   
         ~"PBBPM~".~"BPM001~".~"PEDTM~",   
         ~"PBBPM~".~"BPM001~".~"PEDTE~"  
    FROM ~"PBBPM~".~"BPM001~",   
         ~"PBBPM~".~"BPM003~"  
   WHERE ( ~"PBBPM~".~"BPM001~".~"PCMCD~" = ~"PBBPM~".~"BPM003~".~"OCMCD~" ) and
			( ~"PBBPM~".~"BPM001~".~"PCITN~" = ~"PBBPM~".~"BPM003~".~"OFITN~" ) and  
			( ~"PBBPM~".~"BPM001~".~"PDVSN~" = ~"PBBPM~".~"BPM003~".~"ODVSN~" ) and
         ( ~"PBBPM~".~"BPM001~".~"PLANT~" = ~"PBBPM~".~"BPM003~".~"OPLANT~" ) and
			( ~"PBBPM~".~"BPM001~".~"PCMCD~" = '01' ) AND
         ( ~"PBBPM~".~"BPM001~".~"PDVSN~" = :a_div ) AND  
			( ~"PBBPM~".~"BPM001~".~"PLANT~" = :a_plant ) AND
         ( ~"PBBPM~".~"BPM003~".~"OPITN~" = :a_itno ) AND  
         ( (~"PBBPM~".~"BPM001~".~"PEDTM~" <= ~"PBBPM~".~"BPM001~".~"PEDTE~" AND ~"PBBPM~".~"BPM001~".~"PEDTE~" <> ' ' AND ~"PBBPM~".~"BPM001~".~"PEDTE~" >= :a_date )
          OR  
         ( ~"PBBPM~".~"BPM001~".~"PEDTE~" = ' ' ))
GROUP BY ~"PBBPM~".~"BPM001~".~"PPITN~",   
         ~"PBBPM~".~"BPM001~".~"PCITN~",   
         ~"PBBPM~".~"BPM001~".~"PEDTM~",   
         ~"PBBPM~".~"BPM001~".~"PEDTE~"  
ORDER BY ~"PBBPM~".~"BPM001~".~"PCITN~" ASC,   
         ~"PBBPM~".~"BPM001~".~"PPITN~" ASC   
" arguments=(("a_plant", string),("a_div", string),("a_itno", string),("a_date", string)) )
