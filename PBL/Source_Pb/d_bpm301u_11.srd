 retrieve="  SELECT a.YCCYY,
         a.revno,   
         a.YITNO, 
         (select coalesce(max(itnm),'') from pbbpm.bpm502 x
          where  x.xyear = a.yccyy
          and    x.revno = a.revno
          and    x.itno = a.yitno) itnm,  
         (select coalesce(max(spec),'') from pbbpm.bpm502 x
          where  x.xyear = a.yccyy
          and    x.revno = a.revno
          and    x.itno = a.yitno) spec,  
         a.YVNDSR,
         (select coalesce(max(vndnm),'') from pbpur.pur101 x
          where  x.comltd = '01'
          and    x.vsrno = a.yvndsr) vndnm,     
         a.YPLANT,   
         a.YDIV,   
         a.YGUBUN,   
         a.YGRAD,   
         a.YSRCE,   
         a.YPLAN,   
         a.YCLSB,   
         a.YPDCD, 
          (select coalesce(max(coitname),'') from pbcommon.dac002 x
           where  x.comltd = '01'
           and    x.cogubun = 'DAC160'
           and    x.cocode = a.ypdcd) pdcdnm,   
         a.BOMUNIT,   
         a.XUNIT,   
         a.XUNIT1,   
         a.CONVINV,   
         a.CONVPUR,   
         a.CONVQTY,   
         a.YCHGE,   
         a.YCURR,   
         a.YCSTC, 
         a.YCSTC1,   
         a.YADJDT,   
         a.YCSTR,   
         (case when a.ysrce = '01' then ( case when yplant = 'Y' then
													(select coalesce(max(dper),0) from pbbpm.bpm507 x
													  where x.xyear = a.yccyy
                                         and   x.revno = a.revno
													  and   x.gubun = 'A'
													  and   x.xplant = a.yplant) 
												 else
												   (select coalesce(max(dper),0) from pbbpm.bpm507 x
													 where x.xyear = a.yccyy
                                        and   x.revno = a.revno
													 and   x.gubun = 'B'
													 and   x.xplant = a.ycode
													 and   x.div = '') end) 
                                  else 0 end) dper,
         (case when a.ysrce = '01' then ( case when yplant = 'Y' then
													(select coalesce(max(eper),0) from pbbpm.bpm507 x
													  where x.xyear = a.yccyy
                                         and   x.revno = a.revno
													  and   x.gubun = 'A'
													  and   x.xplant = a.yplant) 
												 else
												   (select coalesce(max(eper),0) from pbbpm.bpm507 x
													 where x.xyear = a.yccyy
                                        and   x.revno = a.revno
													 and   x.gubun = 'B'
													 and   x.xplant = a.ycode
													 and   x.div = '') end) 
                                  else 0 end) eper,
         (select coalesce(max(exch),1) from pbbpm.bpm506 x
						   where  x.xyear = a.yccyy
                     and    x.revno = a.revno
							and    x.curr = a.ycurr) exch,
         a.YCSTD,   
         a.YCSTE,  
         a.YCSTD1,   
         a.YCSTE1,   
         a.YGCST,   
         a.YCODE,   
         a.YALT,   
         a.YEXPT,   
         a.YCODE1,   
         a.YCMCD,   
         a.YDATE,   
         a.YREDT,   
         a.UPDTID,   
         a.UPDTDT,   
         a.CRTDT,
         a.ysakub,
         a.yysung    
    FROM PBBPM.BPM509 a  
   WHERE ( a.YCCYY = :as_xyear ) AND  
         ( a.revno = :as_revno ) AND  
         ( a.YITNO = :as_itno ) AND  
         ( a.YVNDSR = :as_vsrno ) AND  
         ( a.YPLANT like :as_xplant || '%') AND  
         ( a.YDIV like :as_div || '%') AND  
         ( a.YGUBUN = :as_ygubun )    
" update="PBBPM.BPM509" updatewhere=0 updatekeyinplace=yes arguments=(("as_xyear", string),("as_revno", string),("as_itno", string),("as_vsrno", string),("as_xplant", string),("as_div", string),("as_ygubun", string)) )
