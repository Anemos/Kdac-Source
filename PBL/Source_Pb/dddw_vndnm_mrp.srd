  FROM ~"PBPUR~".~"PURDUM~"
	union
  SELECT TRIM(~"PBPUR~".~"PUR101~".~"VSRNO~") AS VSRNO,   
         TRIM(~"PBPUR~".~"PUR101~".~"VNDNM~") AS VNDNM 
    FROM ~"PBPUR~".~"PUR101~"  
