 retrieve="  SELECT TQQCSTANDARDDETAIL.AREACODE,   
         TQQCSTANDARDDETAIL.DIVISIONCODE,   
         TQQCSTANDARDDETAIL.SUPPLIERCODE,   
         TQQCSTANDARDDETAIL.ITEMCODE,   
         TQQCSTANDARDDETAIL.STANDARDREVNO,   
         TQQCSTANDARDDETAIL.ORDERNO,   
         TQQCSTANDARDDETAIL.QCITEM,   
         TQQCSTANDARDDETAIL.DECISIONRANK,   
         TQQCSTANDARDDETAIL.ITEMSPECQUALITY,   
         TQQCSTANDARDDETAIL.SAMPLE,   
         TQQCSTANDARDDETAIL.QCSTRUCTURE,   
         TQQCSTANDARDDETAIL.QCSTIPULATION  
    FROM TQQCSTANDARDDETAIL  
   WHERE ( TQQCSTANDARDDETAIL.AREACODE = :as_areacode ) AND  
         ( TQQCSTANDARDDETAIL.DIVISIONCODE = :as_divisioncpde ) AND  
         ( TQQCSTANDARDDETAIL.SUPPLIERCODE = :as_suppliercode ) AND  
         ( TQQCSTANDARDDETAIL.ITEMCODE = :as_itemcode ) AND  
         ( TQQCSTANDARDDETAIL.STANDARDREVNO = :as_standardrevno ) 

UNION ALL
  
 	SELECT AREACODE, DIVISIONCODE,SUPPLIERCODE,ITEMCODE,STANDARDREVNO,ORDERNO,
					QCITEM, DECISIONRANK, ITEMSPECQUALITY,SAMPLE,QCSTRUCTURE,QCSTIPULATION
			FROM TQQCSTANDARDCHANGEDETAIL
			WHERE AREACODE = :as_areacode AND DIVISIONCODE = :as_divisioncpde AND
					SUPPLIERCODE = :as_suppliercode AND ITEMCODE = :as_itemcode AND
					STANDARDREVNO = :as_standardrevno" update="TQQCSTANDARDDETAIL" updatewhere=1 updatekeyinplace=no arguments=(("as_areacode", string),("as_divisioncpde", string),("as_suppliercode", string),("as_itemcode", string),("as_standardrevno", string)) )
