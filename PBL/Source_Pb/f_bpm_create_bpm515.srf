      ELSE 0 END
    ELSE 0 END,15,6) ) AS MDTHB,
SUM( DECIMAL(CASE WHEN TMP.SRCE <> '01' AND TMP.CLS <> '50' THEN
      CASE WHEN TMP.CUR_SEQNO2 = '99' THEN
        TMP.SUM_YCSTE1 / TMP.CONV_FACTOR * TMP.BPRQT * TMP.BCUMORATE
      ELSE 0 END
    ELSE 0 END,15,6) ) AS MDETC,
//외자재료비 계산
SUM( DECIMAL(CASE WHEN TMP.SRCE = '01' THEN
      CASE WHEN TMP.CUR_SEQNO = '11' THEN
        CASE WHEN TMP.ADECD = 'D' THEN TMP.CURDCST / TMP.CONV_FACTOR * TMP.BPRQT * TMP.BCUMORATE
        ELSE TMP.CURECST / TMP.CONV_FACTOR * TMP.BPRQT * TMP.BCUMORATE END
      ELSE 0 END
    ELSE 0 END,15,6) ) AS MIUSD,
SUM( DECIMAL(CASE WHEN TMP.SRCE = '01' THEN
      CASE WHEN TMP.CUR_SEQNO = '12' THEN
        CASE WHEN TMP.ADECD = 'D' THEN TMP.CURDCST / TMP.CONV_FACTOR * TMP.BPRQT * TMP.BCUMORATE
        ELSE TMP.CURECST / TMP.CONV_FACTOR * TMP.BPRQT * TMP.BCUMORATE END
      ELSE 0 END
    ELSE 0 END,15,6) ) AS MIJPY,
SUM( DECIMAL(CASE WHEN TMP.SRCE = '01' THEN
