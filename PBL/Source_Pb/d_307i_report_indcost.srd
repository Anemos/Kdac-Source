 retrieve="  SELECT ~"PBPDM~".~"BOM109D~".~"FPDCD~", 
         ~"PBCOMMON~".~"DAC007~".~"PRNAME~" ,
			~"PBPDM~".~"BOM109D~".~"FMDNO~",   
			~"PBINV~".~"INV002~".~"ITNM~",          
			~"PBPDM~".~"BOM109D~".~"FDMCST~",   
         ~"PBPDM~".~"BOM109D~".~"FDRCST~",   
         ~"PBPDM~".~"BOM109D~".~"FDACST~",   
         ~"PBPDM~".~"BOM109D~".~"FIMCST~",   
         ~"PBPDM~".~"BOM109D~".~"FIRCST~",   
         ~"PBPDM~".~"BOM109D~".~"FIACST~",   
         ~"PBPDM~".~"BOM109D~".~"FDVOMT~",   
         ~"PBPDM~".~"BOM109D~".~"FDVORT~",   
         ~"PBPDM~".~"BOM109D~".~"FDVOAT~",   
         ~"PBPDM~".~"BOM109D~".~"FTMCST~",   
         ~"PBPDM~".~"BOM109D~".~"FTRCST~",   
         ~"PBPDM~".~"BOM109D~".~"FTACST~",   
         ~"PBPDM~".~"BOM109D~".~"FOSCST~",   
         ~"PBPDM~".~"BOM109D~".~"FISRCE~"  
    FROM ~"PBPDM~".~"BOM109D~",   
         ~"PBINV~".~"INV002~",~"PBCOMMON~".~"DAC007~"
   WHERE ( ~"PBPDM~".~"BOM109D~".~"FMDNO~" = ~"PBINV~".~"INV002~".~"ITNO~" ) and  
         ( ~"PBPDM~".~"BOM109D~".~"FDATE~" = :a_fdate ) AND  
			( ~"PBPDM~".~"BOM109D~".~"FGUBUN~" = :a_comsec ) AND  
         ( ~"PBPDM~".~"BOM109D~".~"FDVSN~" LIKE :a_fdiv ) AND  
         ( ~"PBPDM~".~"BOM109D~".~"FPLANT~" LIKE :a_fplant ) AND
         ( ~"PBPDM~".~"BOM109D~".~"FPDCD~" LIKE :a_fpdcd  )  AND
			( ~"PBPDM~".~"BOM109D~".~"FPDCD~" = ~"PBCOMMON~".~"DAC007~".~"PRPRCD~" ) AND
         ( ~"PBPDM~".~"BOM109D~".~"FCMCD~" = ~"PBCOMMON~".~"DAC007~".~"COMLTD~" )
" arguments=(("a_fplant", string),("a_comsec", string),("a_fdate", string),("a_fdiv", string),("a_fpdcd", string))  sort="BOM109D_fpdcd A BOM109D_fmdno A " )
