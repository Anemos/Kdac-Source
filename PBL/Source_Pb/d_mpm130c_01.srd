 retrieve=" SELECT  TITEMSTORE.MatCls,
			TITEMSTORE.SlipType, 
			TITEMSTORE.Srno,
			TITEMSTORE.Srno1,
			TITEMSTORE.Srno2,
			TITEMSTORE.Rqno, 
			TITEMSTORE.Xplant,
			TITEMSTORE.Div, 
         TITEMSTORE.Itno,
			TITEMSTORE.Itnm,
			TITEMSTORE.Spec,
			TITEMSTORE.Cls,
			TITEMSTORE.Srce,
			TITEMSTORE.Vsrno,
			TITEMSTORE.Vndnm,   
         TITEMSTORE.Tqty4,   
         TITEMSTORE.Tdte4,   
         TITEMSTORE.Xcost,   
         TITEMSTORE.Tramt,   
         TITEMSTORE.LastEmp,   
         TITEMSTORE.LastDate
    FROM TITEMSTORE  
   WHERE ( TITEMSTORE.OrderNo like :a_orderno OR TITEMSTORE.OrderNo IS NULL ) AND
			( TITEMSTORE.PartNo like :a_partno OR TITEMSTORE.PartNo IS NULL ) AND
			( TITEMSTORE.Tdte4 >= :a_fromdt ) AND  
         ( TITEMSTORE.Tdte4 <= :a_todt ) AND
			( TITEMSTORE.MatCls = 'W' )    
" update="TITEMSTORE" updatewhere=0 updatekeyinplace=yes arguments=(("a_orderno", string),("a_partno", string),("a_fromdt", string),("a_todt", string)) )
