 retrieve="  SELECT ~"PBBPM~".~"BPM003~".~"OCMCD~",
         ~"PBBPM~".~"BPM003~".~"OPLANT~", 
         ~"PBBPM~".~"BPM003~".~"ODVSN~",   
         ~"PBBPM~".~"BPM003~".~"OPITN~",   
         ~"PBBPM~".~"BPM003~".~"OFITN~",   
         ~"PBBPM~".~"BPM003~".~"OCHDT~",   
         ~"PBBPM~".~"BPM003~".~"OEDTM~",   
         ~"PBBPM~".~"BPM003~".~"OEDTE~",   
         ~"PBBPM~".~"BPM003~".~"ORATE~",   
         ~"PBBPM~".~"BPM003~".~"OFOCD~",   
         ~"PBBPM~".~"BPM003~".~"OEMNO~",   
         ~"PBBPM~".~"BPM003~".~"OINDT~",   
         ~"PBBPM~".~"BPM003~".~"OCHCD~",
         ~"PBINV~".~"INV101~".~"CLS~", 
         ~"PBINV~".~"INV101~".~"SRCE~",
         ~"PBINV~".~"INV002~".~"ITNM~"  
    FROM ~"PBBPM~".~"BPM003~" ,~"PBINV~".~"INV101~",~"PBINV~".~"INV002~"

   WHERE ( ~"PBBPM~".~"BPM003~".~"OCMCD~" = '01' ) AND
			( ~"PBBPM~".~"BPM003~".~"OPLANT~" = :a_PLANT ) AND
         ( ~"PBBPM~".~"BPM003~".~"ODVSN~" = :a_div ) AND  
         ( ~"PBBPM~".~"BPM003~".~"OPITN~" = :a_itmnbr ) AND  
         ( ~"PBBPM~".~"BPM003~".~"OCMCD~" = ~"PBINV~".~"INV101~".~"COMLTD~" ) AND
  			( ~"PBBPM~".~"BPM003~".~"OPLANT~" = ~"PBINV~".~"INV101~".~"XPLANT~" ) AND
         ( ~"PBBPM~".~"BPM003~".~"ODVSN~" = ~"PBINV~".~"INV101~".~"DIV~" ) AND  
         ( ~"PBBPM~".~"BPM003~".~"OFITN~" = ~"PBINV~".~"INV101~".~"ITNO~" ) AND    
         ( ~"PBBPM~".~"BPM003~".~"OCMCD~" = ~"PBINV~".~"INV002~".~"COMLTD~" ) AND  
         ( ~"PBBPM~".~"BPM003~".~"OFITN~" = ~"PBINV~".~"INV002~".~"ITNO~" ) AND     
         ( (~"PBBPM~".~"BPM003~".~"OEDTM~" <= ~"PBBPM~".~"BPM003~".~"OEDTE~" AND ~"PBBPM~".~"BPM003~".~"OEDTE~" <> ' ' AND ~"PBBPM~".~"BPM003~".~"OEDTE~" >= :a_date )
          OR  
         ( ~"PBBPM~".~"BPM003~".~"OEDTE~" = ' ' ))   
ORDER BY ~"PBBPM~".~"BPM003~".~"OFITN~" ASC   
" arguments=(("a_plant", string),("a_div", string),("a_itmnbr", string),("a_date", string)) )
