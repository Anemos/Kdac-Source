 retrieve="  SELECT TSeqMstItem.CustomerCode,   
         TSeqMstItem.CustomerItemCode,   
         TMSTCUSTITEM.CustItemName,   
         TMSTCUSTITEM.AreaCode,   
         TMSTCUSTITEM.DivisionCode,   
         TSeqMstItem.Itemcode,   
         TSeqMstItem.PartId,   
         TSeqMstItem.AreaCode,   
         TSeqMstItem.DivisionCode,   
         TSeqMstItem.ProductGroup,   
         TSeqMstItem.CarType,   
         TSeqMstItem.Assygubun,   
         TSeqMstItem.Seqgubun,   
         TSeqMstProd.ProductName,   
         TSeqMstProd.SeqGubun,   
         TSEQSHIPQA.OLDSHIPTIME,   
         TSEQSHIPQA.OLDSTATUS  ,
			'                    ' as ordno,
			'        ' as dkdt,
         ' ' as sjcd,
         '        ' as inptdt,
         ' ' as cnlcd,
         ' ' as suse,
         '   ' as cur,
         0.000    as luprc1,
         0        as dcqty1,
         ' ' as stcd,
         '        ' as csrno,'  ' as csrno1 , '  ' as csrno2
    FROM TMSTCUSTITEM,   
         TSeqMstItem,   
         TSeqMstProd,   
         TSEQSHIPQA  
   WHERE ( TMSTCUSTITEM.CustCode = TSeqMstItem.CustomerCode ) and  
         ( TMSTCUSTITEM.CustItemCode = TSeqMstItem.CustomerItemCode ) and  
         ( TSeqMstItem.PartId = TSEQSHIPQA.PARTID ) and  
         ( TSeqMstItem.Itemcode = TSEQSHIPQA.ITEMCODE ) and  
         ( TSeqMstItem.AreaCode = TSeqMstProd.Areacode ) and  
         ( TSeqMstItem.DivisionCode = TSeqMstProd.Divisioncode ) and  
         ( TSeqMstItem.ProductGroup = TSeqMstProd.ProductGroup ) and  
         ( ( TSeqMstProd.SeqGubun = 'B' ) AND  
         ( TSeqMstItem.Seqgubun = 'FA' ) AND  
         ( TSEQSHIPQA.OLDSHIPTIME between :a_fromdate and :a_todate ) AND  
         ( TSeqMstItem.AreaCode = :a_areacode ) AND  
         ( TSeqMstItem.DivisionCode like :a_divisioncode ) AND  
         ( TSeqMstItem.CustomerCode = :a_custcode ) AND  
         ( TSEQSHIPQA.OLDSTATUS = '2' ) )    
" arguments=(("a_fromdate", datetime),("a_todate", datetime),("a_areacode", string),("a_divisioncode", string),("a_custcode", string)) )
