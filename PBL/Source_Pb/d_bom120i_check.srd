    FROM ~"PBPDM~".~"BOM017~",   
         ~"PBINV~".~"INV002~"  ,~"PBCOMMON~".~"DAC003~" A, ~"PBCOMMON~".~"DAC003~" B, ~"PBCOMMON~".~"DAC003~" C 
