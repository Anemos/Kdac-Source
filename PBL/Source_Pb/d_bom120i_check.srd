        	 ( ~"PBPDM~".~"BOM017~".~"XCMCD~" 	= '01' )    and
           ( ~"PBPDM~".~"BOM017~".~"XINEMP~" 	= A.PEEMPNO ) and  
           ( ~"PBPDM~".~"BOM017~".~"XPLEMP~" 	= B.PEEMPNO ) and  
           ( ~"PBPDM~".~"BOM017~".~"XDLEMP~" 	= C.PEEMPNO ) and
		( ~"PBPDM~".~"BOM017~".~"XINTIME~"  	between :ag_intime_from	and :ag_intime_to ) and		
		( ~"PBPDM~".~"BOM017~".~"XPLTIME~"  	between :ag_pltime_from	and :ag_pltime_to ) and		
		( ~"PBPDM~".~"BOM017~".~"XDLTIME~"  	between :ag_dltime_from	and :ag_dltime_to ) and		
