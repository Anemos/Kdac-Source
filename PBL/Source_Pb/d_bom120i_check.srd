	     ( ~"PBPDM~".~"BOM017~".~"XPLCHK~"		like			:ag_plchk	)	and
	     ( ~"PBPDM~".~"BOM017~".~"XDLCHK~" 	like			:ag_dlchk	)	and		
	     ( ~"PBPDM~".~"BOM017~".~"XINEMP~" 	like			:ag_inemp	)	and
	     ( ~"PBPDM~".~"BOM017~".~"XPLEMP~"	like			:ag_plemp	)	and
	     ( ~"PBPDM~".~"BOM017~".~"XDLEMP~" 	like			:ag_dlemp	)
