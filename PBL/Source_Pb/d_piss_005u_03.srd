			TSTOCKCANCEL_INTERFACE.Seqno,
			TSTOCKCANCEL_INTERFACE.InterfaceFlag,  
			TSTOCKCANCEL_INTERFACE.Linecode,   
			case TMSTMODEL.Itemclass 
					when  '10' then
						case TMSTMODEL.Itembuysource 
							when '01' then '2'
							when '02' then '2'
							when '04' then '2'
							when '03' then '3'
							else  '1'
						end
					when '20'  then '2'
					when '30'  then '5'
					when '35'  then '4'
					when '40'  then '2'
					when '50'  then '2'
					else ' '
			end as ItemType
    FROM 	TSTOCKCANCEL_INTERFACE,
			TMSTMODEL
   WHERE 	( TSTOCKCANCEL_INTERFACE.Areacode = :a_areacode ) and
			( TSTOCKCANCEL_INTERFACE.Divisioncode = :a_divisioncode ) and
			( TSTOCKCANCEL_INTERFACE.Itemcode = :a_itemcode ) and
			( TSTOCKCANCEL_INTERFACE.Areacode =TMSTMODEL.Areacode ) and
			( TSTOCKCANCEL_INTERFACE.Divisioncode = TMSTMODEL.Divisioncode ) and
			( TSTOCKCANCEL_INTERFACE.Itemcode = TMSTMODEL.Itemcode ) and
			( TSTOCKCANCEL_INTERFACE.Stockdate = :a_shipdate )  
