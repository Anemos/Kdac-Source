    FROM ~"PBBPM~".~"BPM501~" INNER JOIN ~"PBBPM~".~"BPM515~"
    ON ( ~"PBBPM~".~"BPM501~".~"COMLTD~" = ~"PBBPM~".~"BPM515~".~"COMLTD~" ) and
        ( ~"PBBPM~".~"BPM501~".~"AYEAR~" = ~"PBBPM~".~"BPM515~".~"MYEAR~" ) and
      ( ~"PBBPM~".~"BPM501~".~"AREV~" = ~"PBBPM~".~"BPM515~".~"MBREV~" ) and
      ( ~"PBBPM~".~"BPM501~".~"ACODE~" = ~"PBBPM~".~"BPM515~".~"MGUBUN~" ) and
      ( ~"PBBPM~".~"BPM501~".~"APLANT~" = ~"PBBPM~".~"BPM515~".~"MPLANT~" ) and
      ( ~"PBBPM~".~"BPM501~".~"ADIV~" = ~"PBBPM~".~"BPM515~".~"MDIV~" ) and
      ( ~"PBBPM~".~"BPM501~".~"APDCD~" = ~"PBBPM~".~"BPM515~".~"MMDCD~" ) and
      ( ~"PBBPM~".~"BPM501~".~"AMDNO~" = ~"PBBPM~".~"BPM515~".~"MMDNO~" ) and
      ( ~"PBBPM~".~"BPM501~".~"SEQGB~" = ~"PBBPM~".~"BPM515~".~"MSEQGB~" ) and
		( ~"PBBPM~".~"BPM501~".~"ADECD~" = ~"PBBPM~".~"BPM515~".~"MUSGE~" )
    LEFT OUTER JOIN
        (  SELECT ~"PBPDM~".~"BOM109~".~"FPLANT~" AS FPLANT,
          ~"PBPDM~".~"BOM109~".~"FDVSN~" AS FDVSN,
          ~"PBPDM~".~"BOM109~".~"FMDNO~" AS FMDNO,
        ~"PBPDM~".~"BOM109~".~"FCOSTDIV~" AS FCOSTDIV,
        SUM(CASE WHEN SUBSTRING(~"PBPDM~".~"BOM109~".~"FDATE~",5,2) = '01' THEN ~"PBPDM~".~"BOM109~".~"FDRCST~" - ~"PBPDM~".~"BOM109~".~"FCMCST~" ELSE 0 END) AS FDR01,
