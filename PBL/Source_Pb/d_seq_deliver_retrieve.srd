 retrieve="select b.citno,a.citnm,coalesce(sum(b.srqty),0) as srqty,coalesce(sum(c.caqty),0) as cancelqty,0 as scanqty from pbsle.sle101 a,
( select csrno,citno,srqty,custcd from pbsle.sle302
  where extd = 'DPI' and custcd = :a_custcode and ( frdt between :a_fromdate and :a_todate ) ) b left outer join pbsle.sle304 c
on b.csrno = c.csrno 
where a.custcd = b.custcd and a.citno = b.citno 
group by b.citno,a.citnm
         
" arguments=(("a_custcode", string),("a_fromdate", string),("a_todate", string))  sort="citno A " )
