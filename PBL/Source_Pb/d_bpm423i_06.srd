 retrieve="  SELECT A.~"WPLANT~",   
         A.~"WDVSN~",   
         A.~"WITNO~",    
         A.~"WITEM~",   
         A.~"WPLAN~",   
         A.~"WCLSS~",   
         A.~"WSRCE~",   
         A.~"WPDCD~",   
         A.~"WBOMUM~",
         A.~"WPURUM~", 
         A.~"WCONV~", 
         A.~"WCODE~",   
         A.~"WPAY~",   
         A.~"WVDNM~",   
         A.~"WVDNO~",  
         A.~"WVDKIND~", 
         A.~"WCURR~",   
         A.~"WVHDKDTC~",
         A.~"WVHDNO5~",
         A.~"WCOST~",   
         A.~"WCOSTW~", 
         A.~"WRCPCST~",    
         A.~"WCOSTBOM~",
         A.~"WRCPCSTBOM~",
         A.~"WUSE~",   
         A.~"WQTY1~",   
         A.~"WQTY2~",   
         A.~"WQTY3~",   
         A.~"WQTY4~",   
         A.~"WQTY5~",   
         A.~"WQTY6~",   
         A.~"WQTY7~",   
         A.~"WQTY8~",   
         A.~"WQTY9~",   
         A.~"WQTY10~",   
         A.~"WQTY11~",   
         A.~"WQTY12~",   
         A.~"WYOU~",   
         A.~"WKANBAN~",
		   A.~"WCOSTYR~",
		   A.~"WCOSTYRBOM~",
			A.~"WCOSTOLD~",
			A.~"WCOSTD0A~",
			A.~"WYGCST~",
			A.~"WYCSTORG~",
			A.~"WYGCST1~",
			A.~"WYGCHK~",
			IFNULL(B.~"WQTY~",0) AS INPUTQTY,
			IFNULL(B.~"WAMT~",0) AS INPUTAMT,
			C.CONVPUR,
			C.CONVINV,
			C.CONVQTY,
			IFNULL(B.~"WQTY01~",0) AS WQTY01,
			IFNULL(B.~"WAMT01~",0) AS WAMT01,
			IFNULL(B.~"WQTY02~",0) AS WQTY02,
			IFNULL(B.~"WAMT02~",0) AS WAMT02,
			IFNULL(B.~"WQTY03~",0) AS WQTY03,
			IFNULL(B.~"WAMT03~",0) AS WAMT03,
			IFNULL(B.~"WQTY04~",0) AS WQTY04,
			IFNULL(B.~"WAMT04~",0) AS WAMT04,
			IFNULL(B.~"WQTY05~",0) AS WQTY05,
			IFNULL(B.~"WAMT05~",0) AS WAMT05,
			IFNULL(B.~"WQTY06~",0) AS WQTY06,
			IFNULL(B.~"WAMT06~",0) AS WAMT06,
			IFNULL(B.~"WQTY07~",0) AS WQTY07,
			IFNULL(B.~"WAMT07~",0) AS WAMT07,
			IFNULL(B.~"WQTY08~",0) AS WQTY08,
			IFNULL(B.~"WAMT08~",0) AS WAMT08,
			IFNULL(B.~"WQTY09~",0) AS WQTY09,
			IFNULL(B.~"WAMT09~",0) AS WAMT09,
			IFNULL(B.~"WQTY10~",0) AS WQTY0A,
			IFNULL(B.~"WAMT10~",0) AS WAMT0A,
			IFNULL(B.~"WQTY11~",0) AS WQTY0B,
			IFNULL(B.~"WAMT11~",0) AS WAMT0B,
			IFNULL(B.~"WQTY12~",0) AS WQTY0C,
			IFNULL(B.~"WAMT12~",0) AS WAMT0C,
			A.WMRATE,
			A.WYCURR2,
			A.WCURRATE2
    FROM ~"PBBPM~".~"BPM516B~" A LEFT OUTER JOIN ~"PBBPM~".~"BPM516A~" B
		ON A.COMLTD = B.COMLTD AND A.XYEAR = B.XYEAR AND A.WREV = B.WREV AND
			A.WPLANT = B.WPLANT AND A.WDVSN = B.WDVSN AND
			A.WITNO = B.WITNO AND A.WVDNO = B.WVDNO
		INNER JOIN PBBPM.BPM509 C
		ON A.XYEAR = C.YCCYY AND A.WREV = C.REVNO AND A.WITNO = C.YITNO AND A.WVDNO = C.YVNDSR AND
   		C.YGUBUN = '10' AND ((C.YSRCE = '01' AND C.YGRAD = '1') OR (C.YSRCE <> '01' AND C.YGRAD <> '9'))


   WHERE ( A.~"COMLTD~" = '01' ) AND 
			( A.~"XYEAR~" = :a_year ) AND
			( A.~"WREV~" = :a_rev ) AND
			( A.~"WGUBUN~" = :a_gubun ) AND 
         ( A.~"WPLANT~" like :a_xplant ) AND  
         ( A.~"WDVSN~" like :a_div ) AND  
         ( A.~"WPDCD~" like :a_pdcd ) AND  
         ( A.~"WITNO~" like :a_itno ) " arguments=(("a_year", string),("a_rev", string),("a_gubun", string),("a_xplant", string),("a_div", string),("a_pdcd", string),("a_itno", string)) )
