 retrieve="  SELECT ~"PBBPM~".~"BPM001~".~"PCITN~",   
         ~"PBBPM~".~"BPM001~".~"PPITN~",   
         ~"PBBPM~".~"BPM001~".~"PEDTM~",   
         ~"PBBPM~".~"BPM001~".~"PEDTE~"  
    FROM ~"PBBPM~".~"BPM001~"  
   WHERE (~"PBBPM~".~"BPM001~".~"PCMCD~" = '01') AND
			(~"PBBPM~".~"BPM001~".~"PLANT~" = :a_plant) AND 
         (~"PBBPM~".~"BPM001~".~"PDVSN~" = :a_div) AND  
         (~"PBBPM~".~"BPM001~".~"PCITN~" = :a_itno) AND  
         ( (~"PBBPM~".~"BPM001~".~"PEDTM~" <= ~"PBBPM~".~"BPM001~".~"PEDTE~" AND ~"PBBPM~".~"BPM001~".~"PEDTE~" <> ' ' AND ~"PBBPM~".~"BPM001~".~"PEDTE~" >= :a_date )
          OR  
         ( ~"PBBPM~".~"BPM001~".~"PEDTE~" = ' '  ))
         " arguments=(("a_plant", string),("a_div", string),("a_itno", string),("a_date", string)) )
