 retrieve="  SELECT A.AREACODE,   
         B.AREANAME,
         A.DIVISIONCODE,   
         C.DIVISIONNAME,   
         A.SUPPLIERCODE,   
         D.SUPPLIERKORNAME,
         A.DELIVERYNO,   
         A.MAKEDATE,   
         A.CHARGENAME,   
         A.STANDARDREVNO,   
         A.ITEMCODE,   
         E.ITEMNAME,   
         A.SUPPLIERDELIVERYQTY,   
         A.QCMETHOD,   
         A.SUPPLIERLOTQTY,   
         AS_SUPPLIERLOTNO = H.SUPPLIERLOTNO,   
         AS_LOTQTY = H.LOTQTY,
         A.SUPPLIERDELIVERYQTY AS_SUPPLIERDELIVERYQTY,   
         A.KBCOUNT,
         A.JUDGEFLAG,   
         A.GOODQTY,   
         A.BADQTY,   
         A.BADREASON,   
         A.BADMEMO,   
         A.REMARK,   
			A.KDACREMARK,
         A.INSPECTORCODE,   
         A.CONFIRMCODE,   
         A.CONFIRMFLAG,   
         A.PRINTFLAG,   
         A.QCDATE,   
         A.QCTIME,   
         A.QCLEADTIME,   
         A.QCKBFLAG,   
         A.SLNO,   
         A.DELIVERYDATE,   
         A.DELIVERYTIME,
			A.LASTEMP 
    FROM TQQCRESULT         A INNER JOIN
         TMSTAREA           B
			ON A.AREACODE             = B.AREACODE
         INNER JOIN TMSTDIVISION       C
			ON A.AREACODE             = C.AREACODE
				AND A.DIVISIONCODE         = C.DIVISIONCODE
         INNER JOIN TMSTSUPPLIER       D
			ON A.SUPPLIERCODE         = D.SUPPLIERCODE
         INNER JOIN TMSTITEM           E
			ON A.ITEMCODE             = E.ITEMCODE
			LEFT OUTER JOIN 
         ( SELECT TOP 1 F.AREACODE AREACODE,
						F.DIVISIONCODE DIVISIONCODE,
						F.SUPPLIERCODE SUPPLIERCODE,
						F.DELIVERYNO DELIVERYNO,
						F.ITEMCODE ITEMCODE,
                  F.SUPPLIERLOTNO SUPPLIERLOTNO, 
                  F.LOTQTY        LOTQTY
             FROM TQQCRESULTDETAIL F
            WHERE F.AREACODE    = :as_areacode
				  AND F.DIVISIONCODE= :as_divisioncode
				  AND F.SUPPLIERCODE= :as_suppliercode
				  AND F.DELIVERYNO  = :as_deliveryno
				  AND F.ITEMCODE    = :as_itemcode) H
			ON A.AREACODE             = H.AREACODE
			  AND A.DIVISIONCODE         = H.DIVISIONCODE
			  AND A.SUPPLIERCODE         = H.SUPPLIERCODE
			  AND A.DELIVERYNO           = H.DELIVERYNO
			  AND A.ITEMCODE             = H.ITEMCODE
   WHERE A.AREACODE             = :as_areacode
     AND A.DIVISIONCODE         = :as_divisioncode
     AND A.SUPPLIERCODE         = :as_suppliercode
     AND A.DELIVERYNO           = :as_deliveryno
     AND A.ITEMCODE             = :as_itemcode
 " update="TQQCRESULT" updatewhere=0 updatekeyinplace=no arguments=(("as_areacode", string),("as_divisioncode", string),("as_suppliercode", string),("as_deliveryno", string),("as_itemcode", string)) )
