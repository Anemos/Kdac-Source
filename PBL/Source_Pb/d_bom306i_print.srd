 retrieve="  SELECT ~"PBPDM~".~"BOM113D~".~"ZCMCD~",   
         ~"PBPDM~".~"BOM113D~".~"ZDATE~",   
         ~"PBPDM~".~"BOM113D~".~"ZPLANT~",   
         ~"PBPDM~".~"BOM113D~".~"ZDIV~",   
         ~"PBPDM~".~"BOM113D~".~"ZMDCD~",   
         ~"PBPDM~".~"BOM113D~".~"ZMDNO~",   
         ~"PBPDM~".~"BOM113D~".~"ZSERIAL~",   
         ~"PBPDM~".~"BOM113D~".~"ZLEVEL~",   
         ~"PBPDM~".~"BOM113D~".~"ZITNO~",   
         ~"PBPDM~".~"BOM113D~".~"ZREV~",   
         ~"PBPDM~".~"BOM113D~".~"ZDESC~",   
         ~"PBPDM~".~"BOM113D~".~"ZSPEC~",   
         ~"PBPDM~".~"BOM113D~".~"ZUNIT1~",   
         ~"PBPDM~".~"BOM113D~".~"ZUNITQTY~",   
         ~"PBPDM~".~"BOM113D~".~"ZCVTFACT~",   
         ~"PBPDM~".~"BOM113D~".~"ZUNIT2~",   
         ~"PBPDM~".~"BOM113D~".~"ZABCCODE~",   
         ~"PBPDM~".~"BOM113D~".~"ZISSCODE~",   
         ~"PBPDM~".~"BOM113D~".~"ZWHLCODE~",   
         ~"PBPDM~".~"BOM113D~".~"ZLOTSIZE~",   
         ~"PBPDM~".~"BOM113D~".~"ZLEADTIME~",	
			CASE WHEN ~"PBPDM~".~"BOM113D~".~"ZYGCHK~" = 'Y' 
				THEN ~"PBPDM~".~"BOM113D~".~"ZSCOUTCOST~"
				ELSE ~"PBPDM~".~"BOM113D~".~"ZOUTCOST~" END AS ZMOVCOST,   
			CASE WHEN ~"PBPDM~".~"BOM113D~".~"ZYGCHK~" = 'Y' 
				THEN ~"PBPDM~".~"BOM113D~".~"ZSCRCPCOST~"
				ELSE ~"PBPDM~".~"BOM113D~".~"ZRCPCOST~" END AS ZRCPCOST, 	
			~"PBPDM~".~"BOM113D~".~"ZOUTCOST~", 
			~"PBPDM~".~"BOM113D~".~"ZSCMOVCOST~",   
         ~"PBPDM~".~"BOM113D~".~"ZSCRCPCOST~", 
			~"PBPDM~".~"BOM113D~".~"ZSCOUTCOST~",   
         ~"PBPDM~".~"BOM113D~".~"ZWKCT~",   
         ~"PBPDM~".~"BOM113D~".~"ZGRAD~",   
         ~"PBPDM~".~"BOM113D~".~"ZALTNO~",   
         ~"PBPDM~".~"BOM113D~".~"ZFMDT~",   
         ~"PBPDM~".~"BOM113D~".~"ZTODT~",   
         CASE WHEN ~"PBINV~".~"INV101~".~"SRCE~" = '03' THEN 'N' ELSE 'Y' END AS ZCALC,   
         ~"PBINV~".~"INV101~".~"CLS~",   
         ~"PBINV~".~"INV101~".~"SRCE~",   
         ~"PBCOMMON~".~"DAC007~".~"PRNAME~",
			A.COITNAME AS PLANTNAME,
			B.COITNAME AS DIVNAME,
			~"PBPDM~".~"BOM113D~".~"ZCOMCD~",
			:ps_applygubun AS APPLYGUBUN
    FROM ~"PBPDM~".~"BOM113D~",   
         ~"PBINV~".~"INV101~",
         ~"PBCOMMON~".~"DAC007~",  
         ~"PBCOMMON~".~"DAC002~" A, 
         ~"PBCOMMON~".~"DAC002~" B
   WHERE ( ~"PBPDM~".~"BOM113D~".~"ZCMCD~" 	= ~"PBINV~".~"INV101~".~"COMLTD~" ) and  
         ( ~"PBPDM~".~"BOM113D~".~"ZPLANT~" 	= ~"PBINV~".~"INV101~".~"XPLANT~" ) and  
         ( ~"PBPDM~".~"BOM113D~".~"ZDIV~" 		= ~"PBINV~".~"INV101~".~"DIV~" ) and  
         ( ~"PBPDM~".~"BOM113D~".~"ZITNO~" 	= ~"PBINV~".~"INV101~".~"ITNO~" ) and   
         ( ~"PBPDM~".~"BOM113D~".~"ZCMCD~" 	= ~"PBCOMMON~".~"DAC007~".~"COMLTD~" ) and  
         ( ~"PBPDM~".~"BOM113D~".~"ZMDCD~" 	= ~"PBCOMMON~".~"DAC007~".~"PRPRCD~" ) and  
         ( ( ~"PBPDM~".~"BOM113D~".~"ZCMCD~" 	= '01' ) AND  
         ( ~"PBPDM~".~"BOM113D~".~"ZDATE~" 	= :ps_year ) AND  
         ( ~"PBPDM~".~"BOM113D~".~"ZPLANT~" 	like :ps_xplant ) AND  
         ( ~"PBPDM~".~"BOM113D~".~"ZDIV~" 		like :ps_div ) AND  
         ( trim(~"PBPDM~".~"BOM113D~".~"ZMDCD~") like :ps_pdcd ) AND  
         ( trim(~"PBPDM~".~"BOM113D~".~"ZMDNO~") like :ps_itno ) )   AND
         ( ~"PBPDM~".~"BOM113D~".~"ZCMCD~" = A.COMLTD AND ~"PBPDM~".~"BOM113D~".~"ZPLANT~" = TRIM(A.COCODE) AND A.COGUBUN = 'EIS040' ) AND
			( ~"PBPDM~".~"BOM113D~".~"ZCMCD~" = B.COMLTD AND ~"PBPDM~".~"BOM113D~".~"ZDIV~"   = TRIM(B.COCODE) AND B.COGUBUN = 'EIS040' )
 
" arguments=(("ps_year", string),("ps_xplant", string),("ps_div", string),("ps_pdcd", string),("ps_itno", string),("ps_date", string),("ps_applygubun", string))  sort="zplant A zdiv A zmdcd A zmdno A zserial A " )
