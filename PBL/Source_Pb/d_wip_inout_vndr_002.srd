 column=(type=decimal(0) updatewhereclause=yes name=com_amt dbname="COM_AMT" )
 retrieve="SELECT XPLANT, DIV, ITNO,CLS,SRCE,VSRNO, SLIPTYPE, XUSE, RTNGUB,SUM(TQTY4) AS COM_QTY, SUM(TRAMT) AS COM_AMT
FROM PBINV.INV401
WHERE (COMLTD = :a_cmcd) AND (XPLANT = :a_plant) AND (DIV = :a_dvsn) 
		AND (TDTE4 >= :a_fromdt) AND (TDTE4 <= :a_todt)
		AND ((SLIPTYPE IN ('IS','RS','IP')) AND CLS IN ('10','40','50'))
      AND XUSE IN ('04','07')
		AND SUBSTR(VSRNO,1,1) = 'D' AND TQTY4 <> 0
GROUP BY XPLANT,DIV,ITNO,CLS,SRCE,VSRNO,SLIPTYPE, XUSE, RTNGUB

UNION ALL

SELECT XPLANT, DIV, ITNO,CLS,SRCE,VSRNO, SLIPTYPE, XUSE, RTNGUB,SUM(TQTY4) AS COM_QTY, SUM(TRAMT) AS COM_AMT
FROM PBINV.INV401
WHERE (COMLTD = :a_cmcd) AND (XPLANT = :a_plant) AND (DIV = :a_dvsn) 
		AND (TDTE4 >= :a_fromdt) AND (TDTE4 <= :a_todt)
		AND ((SLIPTYPE IN ('IS','RS','IP')) AND CLS IN ('10','40','50'))
      AND XUSE IN ('04','07')
		AND SUBSTR(VSRNO,1,1) = 'D' AND TQTY4 = 0
GROUP BY XPLANT,DIV,ITNO,CLS,SRCE,VSRNO,SLIPTYPE, XUSE, RTNGUB

UNION ALL

SELECT WDPLANT, WDDVSN, WDITNO,WDITCL,WDSRCE,WDCHDPT, WDSLTY,WDUSGE,'', SUM(WDCHQT) AS COM_QTY, SUM(0) AS COM_AMT
FROM PBWIP.WIP004
WHERE (WDCMCD = :a_cmcd) AND (WDPLANT = :a_plant) AND (WDDVSN = :a_dvsn) 
		AND (WDDATE >= :a_fromdt) AND (WDDATE <= :a_todt)
		AND WDIOCD = '2' AND (WDSLTY IN ('IE','WR','WC','WX'))
GROUP BY WDPLANT,WDDVSN,WDITNO,WDITCL,WDSRCE,WDCHDPT,WDSLTY,WDUSGE

UNION ALL

SELECT WBPLANT, WBDVSN, WBITNO,WBITCL,WBSRCE,WBORCT, ' ', ' ', ' ', 0 AS COM_QTY, 0 AS COM_AMT
FROM PBWIP.WIP002	
WHERE (WBCMCD = :a_cmcd) AND (WBPLANT = :a_plant ) AND (WBDVSN = :a_dvsn ) 
		AND (WBYEAR = :a_year) AND (WBMONTH = :a_month)
		AND (WBIOCD = '2')
		AND ( NOT ( WBBGQT = 0 AND WBBGAT1 = 0 AND WBBGAT2 = 0 AND WBINQT = 0 AND WBINAT1 = 0 AND WBINAT2 = 0 AND
                 WBINAT3 = 0 AND WBINAT4 = 0 AND WBUSQT1 = 0 AND WBUSAT1 = 0 AND WBUSQT2 = 0 AND WBUSAT2 = 0 AND
					  WBUSQT3 = 0 AND WBUSAT3 = 0 AND  WBUSQT4 = 0 AND WBUSAT4 = 0 AND WBUSQT5 = 0 AND WBUSAT5 = 0 AND 
					  WBUSQT6 = 0 AND WBUSAT6 = 0 AND WBUSQT7 = 0 AND WBUSAT7 = 0 AND  WBUSQT8 = 0 AND WBUSAT8 = 0 AND
					  WBUSQTA = 0 AND WBUSATA = 0 ))

" arguments=(("a_cmcd", string),("a_plant", string),("a_dvsn", string),("a_fromdt", string),("a_todt", string),("a_year", string),("a_month", string))  sort="itno A vsrno A sliptype A " )
