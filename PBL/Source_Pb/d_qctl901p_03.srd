 retrieve="  SELECT A.~"DIV~",
         {fn substring(B.~"PDCD~",1,2)} as pdcd,
         sum(A.~"DCQTY~") as tot_qty,
         sum(A.~"SLAMTW~") / 1000 as tot_wamt,
         sum(A.qty1) qty1,
         sum(A.qty2) qty2,
         sum(A.qty3) qty3,
         sum(A.qty4) qty4,
         sum(A.qty5) qty5,
         sum(A.qty6) qty6,
         sum(A.qty7) qty7,
         sum(A.qty8) qty8,
         sum(A.qty9) qty9,
         sum(A.qty10) qty10,
         sum(A.qty11) qty11,
         sum(A.qty12) qty12,
         sum(A.wamt1) / 1000 wamt1,
         sum(A.wamt2) / 1000 wamt2,
         sum(A.wamt3) / 1000 wamt3,
         sum(A.wamt4) / 1000 wamt4,
         sum(A.wamt5) / 1000 wamt5,
         sum(A.wamt6) / 1000 wamt6,
         sum(A.wamt7) / 1000 wamt7,
         sum(A.wamt8) / 1000 wamt8,
         sum(A.wamt9) / 1000 wamt9,
         sum(A.wamt10) / 1000 wamt10,
         sum(A.wamt11) / 1000 wamt11,
         sum(A.wamt12) / 1000 wamt12,
         max('    ',:a_s_year) as tit_year,
         A.~"XPLANT~"  
    FROM (SELECT COMLTD, CUSTCD, XPLANT, DIV, CITNO, PDCD, 
                 DCQTY,  SLAMTW,
					  CASE WHEN SUBSTR(SLSLDT, 5, 2) = '01' THEN dcqty ELSE 0  END qty1,
					  CASE WHEN SUBSTR(SLSLDT, 5, 2) = '02' THEN dcqty ELSE 0  END qty2,
					  CASE WHEN SUBSTR(SLSLDT, 5, 2) = '03' THEN dcqty ELSE 0  END qty3,
					  CASE WHEN SUBSTR(SLSLDT, 5, 2) = '04' THEN dcqty ELSE 0  END qty4,
					  CASE WHEN SUBSTR(SLSLDT, 5, 2) = '05' THEN dcqty ELSE 0  END qty5,
					  CASE WHEN SUBSTR(SLSLDT, 5, 2) = '06' THEN dcqty ELSE 0  END qty6,
					  CASE WHEN SUBSTR(SLSLDT, 5, 2) = '07' THEN dcqty ELSE 0  END qty7,
					  CASE WHEN SUBSTR(SLSLDT, 5, 2) = '08' THEN dcqty ELSE 0  END qty8,
					  CASE WHEN SUBSTR(SLSLDT, 5, 2) = '09' THEN dcqty ELSE 0  END qty9,
					  CASE WHEN SUBSTR(SLSLDT, 5, 2) = '10' THEN dcqty ELSE 0  END qty10,
					  CASE WHEN SUBSTR(SLSLDT, 5, 2) = '11' THEN dcqty ELSE 0  END qty11,
					  CASE WHEN SUBSTR(SLSLDT, 5, 2) = '12' THEN dcqty ELSE 0  END qty12,
					  CASE WHEN SUBSTR(SLSLDT, 5, 2) = '01' THEN slamtw ELSE 0  END wamt1,
					  CASE WHEN SUBSTR(SLSLDT, 5, 2) = '02' THEN slamtw ELSE 0  END wamt2,
					  CASE WHEN SUBSTR(SLSLDT, 5, 2) = '03' THEN slamtw ELSE 0  END wamt3,
					  CASE WHEN SUBSTR(SLSLDT, 5, 2) = '04' THEN slamtw ELSE 0  END wamt4,
					  CASE WHEN SUBSTR(SLSLDT, 5, 2) = '05' THEN slamtw ELSE 0  END wamt5,
					  CASE WHEN SUBSTR(SLSLDT, 5, 2) = '06' THEN slamtw ELSE 0  END wamt6,
					  CASE WHEN SUBSTR(SLSLDT, 5, 2) = '07' THEN slamtw ELSE 0  END wamt7,
					  CASE WHEN SUBSTR(SLSLDT, 5, 2) = '08' THEN slamtw ELSE 0  END wamt8,
					  CASE WHEN SUBSTR(SLSLDT, 5, 2) = '09' THEN slamtw ELSE 0  END wamt9,
					  CASE WHEN SUBSTR(SLSLDT, 5, 2) = '10' THEN slamtw ELSE 0  END wamt10,
					  CASE WHEN SUBSTR(SLSLDT, 5, 2) = '11' THEN slamtw ELSE 0  END wamt11,
					  CASE WHEN SUBSTR(SLSLDT, 5, 2) = '12' THEN slamtw ELSE 0  END wamt12
            FROM ~"PBSLE~".~"SLE501~"
           WHERE ~"COMLTD~"  = '01' and
					  ~"STCD~"    = '1'  and
					  ~"DTYPE~"   <> 'A' and
					  ~"SLSLDT~"  LIKE :a_s_year || '%' AND
					  ~"KITCD~"   <>   'C'  and
					  ~"SUSE~"    LIKE :a_s_duse || '%') A,
         ~"PBSLE~".~"SLE101~" B  
   WHERE CASE WHEN (B.~"SPCD~" = 'M') THEN 4
              ELSE LENGTH(B.~"PDCD~")
         END = 4 and
         ( A.~"COMLTD~"  = B.~"COMLTD~" ) and
         ( A.~"CUSTCD~"  = B.~"CUSTCD~" ) and
         ( A.~"CITNO~"   = B.~"CITNO~" )  
  GROUP BY A.~"XPLANT~", A.~"DIV~",
           {fn substring(B.~"PDCD~",1,2)}
" arguments=(("a_s_duse", string),("a_s_year", string))  sort="sle501_xplant A sle501_div A " )
