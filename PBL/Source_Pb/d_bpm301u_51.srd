 retrieve="  SELECT 0 no,
         a.YITNO,
         a.YVNDSR,   
         a.YPLANT,   
         a.YDIV,   
         a.YPLAN,   
         a.YCHGE, 
         a.YCODE1,   
         a.YCSTR, 
         a.YCSTC1,    
         a.YGCST,   
         a.YCMCD,   

         a.YGRAD,   
         a.YSRCE,   
         a.YCLSB,   
         a.YPDCD,   
         a.YGUBUN,   
         a.BOMUNIT,   
         a.XUNIT,   
         a.XUNIT1,   
         a.CONVINV,   
         a.CONVPUR,   
         a.CONVQTY,   
 
         a.YCURR,   
         a.YCSTC,  
          
         a.YADJDT,   
         
         a.YCSTD,   
         a.YCSTE,   
         a.YCSTD1,   
         a.YCSTE1,   
         
         a.YCODE,   
         a.YALT,   
         a.YEXPT,   
           
         
         a.YDATE,   
         a.YREDT,   
         a.UPDTID,   
         a.UPDTDT,   
         a.CRTDT,
         a.Yccyy,  
         (select coalesce(max(itnm),'') from pbbpm.bpm502 x
          where x.xyear = a.yccyy
          and   x.itno = a.yitno) itnm,   
         (select coalesce(max(spec),'') from pbbpm.bpm502 x
          where x.xyear = a.yccyy
          and   x.itno = a.yitno) spec  
    FROM PBBPM.BPM509 a 
   WHERE ( a.YCCYY = :as_xyear ) AND  
         ( a.YPLANT like :as_xplant || '%' ) AND  
         ( a.YDIV like :as_div || '%' ) AND  
         ( a.YITNO like :as_itno || '%' ) AND  
         ( a.YVNDSR like :as_vsrno || '%' )
   and   (:as_srce = '01' and a.ysrce = '01' or :as_srce <> '01' and a.ysrce <> '01')      
" update="PBBPM.BPM509" updatewhere=0 updatekeyinplace=yes arguments=(("as_xyear", string),("as_xplant", string),("as_div", string),("as_itno", string),("as_vsrno", string),("as_srce", string)) )
