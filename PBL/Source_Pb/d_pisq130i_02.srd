 retrieve="SELECT AS_AREACODE, ISNULL(SUM(M_I11), 0) AS AS_I11, ISNULL(SUM(M_I10), 0) AS AS_I10, ISNULL(SUM(M_I09), 0) AS AS_I09, ISNULL(SUM(M_I08), 0) AS AS_I08, ISNULL(SUM(M_I07), 0) AS AS_I07, ISNULL(SUM(M_I06), 0) AS AS_I06, ISNULL(SUM(M_I05), 0) AS AS_I05, ISNULL(SUM(M_I04), 0) AS AS_I04, ISNULL(SUM(M_I03), 0) AS AS_I03, ISNULL(SUM(M_I02), 0) AS AS_I02, ISNULL(SUM(M_I01), 0) AS AS_I01, ISNULL(SUM(M_I00), 0) AS AS_I00,
                    ISNULL(SUM(M_B11), 0) AS AS_B11, ISNULL(SUM(M_B10), 0) AS AS_B10, ISNULL(SUM(M_B09), 0) AS AS_B09, ISNULL(SUM(M_B08), 0) AS AS_B08, ISNULL(SUM(M_B07), 0) AS AS_B07, ISNULL(SUM(M_B06), 0) AS AS_B06, ISNULL(SUM(M_B05), 0) AS AS_B05, ISNULL(SUM(M_B04), 0) AS AS_B04, ISNULL(SUM(M_B03), 0) AS AS_B03, ISNULL(SUM(M_B02), 0) AS AS_B02, ISNULL(SUM(M_B01), 0) AS AS_B01, ISNULL(SUM(M_B00), 0) AS AS_B00
 FROM (
		SELECT A.AREACODE AS AS_AREACODE,
				 CASE WHEN CONVERT(CHAR(10), DATEADD(month, -11, :as_qcdate_fm), 102) <= A.QCDATE AND A.QCDATE <= CONVERT(CHAR(10), DATEADD(month, -11, :as_qcdate_to), 102) THEN count(*) END AS M_I11,
				 CASE WHEN CONVERT(CHAR(10), DATEADD(month, -10, :as_qcdate_fm), 102) <= A.QCDATE AND A.QCDATE <= CONVERT(CHAR(10), DATEADD(month, -10, :as_qcdate_to), 102) THEN count(*) END AS M_I10,
				 CASE WHEN CONVERT(CHAR(10), DATEADD(month, -09, :as_qcdate_fm), 102) <= A.QCDATE AND A.QCDATE <= CONVERT(CHAR(10), DATEADD(month, -09, :as_qcdate_to), 102) THEN count(*) END AS M_I09,
				 CASE WHEN CONVERT(CHAR(10), DATEADD(month, -08, :as_qcdate_fm), 102) <= A.QCDATE AND A.QCDATE <= CONVERT(CHAR(10), DATEADD(month, -08, :as_qcdate_to), 102) THEN count(*) END AS M_I08,
				 CASE WHEN CONVERT(CHAR(10), DATEADD(month, -07, :as_qcdate_fm), 102) <= A.QCDATE AND A.QCDATE <= CONVERT(CHAR(10), DATEADD(month, -07, :as_qcdate_to), 102) THEN count(*) END AS M_I07,
				 CASE WHEN CONVERT(CHAR(10), DATEADD(month, -06, :as_qcdate_fm), 102) <= A.QCDATE AND A.QCDATE <= CONVERT(CHAR(10), DATEADD(month, -06, :as_qcdate_to), 102) THEN count(*) END AS M_I06,
				 CASE WHEN CONVERT(CHAR(10), DATEADD(month, -05, :as_qcdate_fm), 102) <= A.QCDATE AND A.QCDATE <= CONVERT(CHAR(10), DATEADD(month, -05, :as_qcdate_to), 102) THEN count(*) END AS M_I05,
				 CASE WHEN CONVERT(CHAR(10), DATEADD(month, -04, :as_qcdate_fm), 102) <= A.QCDATE AND A.QCDATE <= CONVERT(CHAR(10), DATEADD(month, -04, :as_qcdate_to), 102) THEN count(*) END AS M_I04,
				 CASE WHEN CONVERT(CHAR(10), DATEADD(month, -03, :as_qcdate_fm), 102) <= A.QCDATE AND A.QCDATE <= CONVERT(CHAR(10), DATEADD(month, -03, :as_qcdate_to), 102) THEN count(*) END AS M_I03,
				 CASE WHEN CONVERT(CHAR(10), DATEADD(month, -02, :as_qcdate_fm), 102) <= A.QCDATE AND A.QCDATE <= CONVERT(CHAR(10), DATEADD(month, -02, :as_qcdate_to), 102) THEN count(*) END AS M_I02,
				 CASE WHEN CONVERT(CHAR(10), DATEADD(month, -01, :as_qcdate_fm), 102) <= A.QCDATE AND A.QCDATE <= CONVERT(CHAR(10), DATEADD(month, -01, :as_qcdate_to), 102) THEN count(*) END AS M_I01,
				 CASE WHEN                                       :as_qcdate_fm        <= A.QCDATE AND A.QCDATE <=                                       :as_qcdate_to        THEN count(*) END AS M_I00,
				 0                                                                                                                                                                           AS M_B11,
				 0                                                                                                                                                                           AS M_B10,
				 0                                                                                                                                                                           AS M_B09,
				 0                                                                                                                                                                           AS M_B08,
				 0                                                                                                                                                                           AS M_B07,
				 0                                                                                                                                                                           AS M_B06,
				 0                                                                                                                                                                           AS M_B05,
				 0                                                                                                                                                                           AS M_B04,
				 0                                                                                                                                                                           AS M_B03,
				 0                                                                                                                                                                           AS M_B02,
				 0                                                                                                                                                                           AS M_B01,
				 0                                                                                                                                                                           AS M_B00
		  FROM TQQCRESULT  A
       WHERE A.AREACODE	    =	   :as_areacode
         AND A.DIVISIONCODE =	   :as_divisioncode
         AND A.SUPPLIERCODE LIKE :as_suppliercode
         AND A.ITEMCODE     LIKE :as_itemcode
         AND A.QCDATE		 >=   CONVERT(CHAR(10), DATEADD(month, -11, :as_qcdate_fm), 102)
         AND A.QCDATE		 <=   :as_qcdate_to
		 GROUP BY A.AREACODE, A.QCDATE

        UNION ALL
	
		SELECT A.AREACODE AS AS_AREACODE,
				 0                                                                                                                                                                           AS M_I11,
				 0                                                                                                                                                                           AS M_I10,
				 0                                                                                                                                                                           AS M_I09,
				 0                                                                                                                                                                           AS M_I08,
				 0                                                                                                                                                                           AS M_I07,
				 0                                                                                                                                                                           AS M_I06,
				 0                                                                                                                                                                           AS M_I05,
				 0                                                                                                                                                                           AS M_I04,
				 0                                                                                                                                                                           AS M_I03,
				 0                                                                                                                                                                           AS M_I02,
				 0                                                                                                                                                                           AS M_I01,
				 0                                                                                                                                                                           AS M_I00,
				 CASE WHEN CONVERT(CHAR(10), DATEADD(month, -11, :as_qcdate_fm), 102) <= A.QCDATE AND A.QCDATE <= CONVERT(CHAR(10), DATEADD(month, -11, :as_qcdate_to), 102) THEN count(*) END AS M_B11,
				 CASE WHEN CONVERT(CHAR(10), DATEADD(month, -10, :as_qcdate_fm), 102) <= A.QCDATE AND A.QCDATE <= CONVERT(CHAR(10), DATEADD(month, -10, :as_qcdate_to), 102) THEN count(*) END AS M_B10,
				 CASE WHEN CONVERT(CHAR(10), DATEADD(month, -09, :as_qcdate_fm), 102) <= A.QCDATE AND A.QCDATE <= CONVERT(CHAR(10), DATEADD(month, -09, :as_qcdate_to), 102) THEN count(*) END AS M_B09,
				 CASE WHEN CONVERT(CHAR(10), DATEADD(month, -08, :as_qcdate_fm), 102) <= A.QCDATE AND A.QCDATE <= CONVERT(CHAR(10), DATEADD(month, -08, :as_qcdate_to), 102) THEN count(*) END AS M_B08,
				 CASE WHEN CONVERT(CHAR(10), DATEADD(month, -07, :as_qcdate_fm), 102) <= A.QCDATE AND A.QCDATE <= CONVERT(CHAR(10), DATEADD(month, -07, :as_qcdate_to), 102) THEN count(*) END AS M_B07,
				 CASE WHEN CONVERT(CHAR(10), DATEADD(month, -06, :as_qcdate_fm), 102) <= A.QCDATE AND A.QCDATE <= CONVERT(CHAR(10), DATEADD(month, -06, :as_qcdate_to), 102) THEN count(*) END AS M_B06,
				 CASE WHEN CONVERT(CHAR(10), DATEADD(month, -05, :as_qcdate_fm), 102) <= A.QCDATE AND A.QCDATE <= CONVERT(CHAR(10), DATEADD(month, -05, :as_qcdate_to), 102) THEN count(*) END AS M_B05,
				 CASE WHEN CONVERT(CHAR(10), DATEADD(month, -04, :as_qcdate_fm), 102) <= A.QCDATE AND A.QCDATE <= CONVERT(CHAR(10), DATEADD(month, -04, :as_qcdate_to), 102) THEN count(*) END AS M_B04,
				 CASE WHEN CONVERT(CHAR(10), DATEADD(month, -03, :as_qcdate_fm), 102) <= A.QCDATE AND A.QCDATE <= CONVERT(CHAR(10), DATEADD(month, -03, :as_qcdate_to), 102) THEN count(*) END AS M_B03,
				 CASE WHEN CONVERT(CHAR(10), DATEADD(month, -02, :as_qcdate_fm), 102) <= A.QCDATE AND A.QCDATE <= CONVERT(CHAR(10), DATEADD(month, -02, :as_qcdate_to), 102) THEN count(*) END AS M_B02,
				 CASE WHEN CONVERT(CHAR(10), DATEADD(month, -01, :as_qcdate_fm), 102) <= A.QCDATE AND A.QCDATE <= CONVERT(CHAR(10), DATEADD(month, -01, :as_qcdate_to), 102) THEN count(*) END AS M_B01,
				 CASE WHEN                                       :as_qcdate_fm        <= A.QCDATE AND A.QCDATE <=                                       :as_qcdate_to        THEN count(*) END AS M_B00
		  FROM TQQCRESULT  A
       WHERE A.AREACODE	    =	   :as_areacode
         AND A.DIVISIONCODE =	   :as_divisioncode
         AND A.SUPPLIERCODE LIKE :as_suppliercode
         AND A.ITEMCODE     LIKE :as_itemcode
         AND A.QCDATE		 >=   CONVERT(CHAR(10), DATEADD(month, -11, :as_qcdate_fm), 102)
         AND A.QCDATE		 <=   :as_qcdate_to
         AND A.JUDGEFLAG    IN   ('2', '3')
       GROUP BY A.AREACODE, A.QCDATE
) AS TMP
GROUP BY AS_AREACODE
" arguments=(("as_areacode", string),("as_divisioncode", string),("as_suppliercode", string),("as_itemcode", string),("as_qcdate_fm", string),("as_qcdate_to", string)) )
