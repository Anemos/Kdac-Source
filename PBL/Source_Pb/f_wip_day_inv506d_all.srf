		 	( "PBPDM"."BOM010"."AYEAR"||"PBPDM"."BOM010"."AMONT" = "PBPDM"."BOM016"."FDATE" ) and
			( "PBINV"."INV101"."COMLTD" = "PBCOMMON"."DAC007"."COMLTD" ) and  
