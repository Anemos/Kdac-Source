		"PBCOMMON"."DAC007"."PRNAME" AS PRNAME,
      "PBINV"."INV002"."ITNM" AS ITNM,
		"PBINV"."INV002"."SPEC" AS SPEC,
		"PBINV"."INV101"."CLS" AS CLS,
		"PBINV"."INV101"."SRCE" AS SRCE,
