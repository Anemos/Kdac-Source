 retrieve="  SELECT TPARTKBSTATUS.PartKBNo,   
         TPARTKBSTATUS.AreaCode,   
         TPARTKBSTATUS.DivisionCode,   
         TPARTKBSTATUS.SupplierCode,   
         TPARTKBSTATUS.ItemCode,   
         TPARTKBSTATUS.ApplyFrom,   
         TPARTKBSTATUS.PartType,   
         TPARTKBSTATUS.RePrintFlag,   
         TPARTKBSTATUS.PartKBGubun,   
         TPARTKBSTATUS.KBActiveGubun,   
         TPARTKBSTATUS.PartKBStatus,   
         TPARTKBSTATUS.KBOrderPossible,   
         TPARTKBSTATUS.RackQty,   
         TPARTKBSTATUS.OrderChangeFlag,   
         TPARTKBSTATUS.PartOrderCancel,   
         TPARTKBSTATUS.PartReceiptCancel,   
         TPARTKBSTATUS.UseCenterGubun,   
         TPARTKBSTATUS.UseCenter,   
         TPARTKBSTATUS.PartObeyDateFlag,   
         TPARTKBSTATUS.PartObeyTimeFlag,   
         TPARTKBSTATUS.PartOrderDate,   
         TPARTKBSTATUS.PartOrderTime,   
         TPARTKBSTATUS.PartForeCastDate,   
         TPARTKBSTATUS.PartForecastEditNo,   
         TPARTKBSTATUS.PartForecastTime,   
         TPARTKBSTATUS.PartReceiptDate,   
         TPARTKBSTATUS.PartEditNo,   
         TPARTKBSTATUS.PartReceiptTime,   
         TPARTKBSTATUS.PartIncomeDate,   
         TPARTKBSTATUS.PartIncomeTime,   
         TPARTKBSTATUS.PartOrderNo,   
         TPARTKBSTATUS.DeliveryNo,   
         TPARTKBSTATUS.PartKBCreateDate,   
         TPARTKBSTATUS.PartKBPrintDate,   
         TPARTKBSTATUS.OrderChangeTime,   
         TPARTKBSTATUS.OrderChangeCode,   
         TPARTKBSTATUS.ChangeForecastDate,   
         TPARTKBSTATUS.ChangeForecastEditNo,   
         TPARTKBSTATUS.ChangeForecastTime,   
         TPARTKBSTATUS.LastEmp,   
         TPARTKBSTATUS.LastDate  
    FROM TPARTKBSTATUS  
   WHERE ( TPARTKBSTATUS.AreaCode = :as_areaCode ) AND  
         ( TPARTKBSTATUS.DivisionCode = :as_divCode ) AND  
         ( TPARTKBSTATUS.SupplierCode = :as_suppCode ) AND  
         ( TPARTKBSTATUS.ItemCode = :as_itemCode ) AND  
         ( TPARTKBSTATUS.ApplyFrom <= Convert(Char(10), getDate(), 112)) AND  
         ( TPARTKBSTATUS.RePrintFlag = 'Y' ) And
         ( ( TPARTKBSTATUS.KBActiveGubun = 'S' ) or
           ( ( TPARTKBSTATUS.KBActiveGubun = 'A' ) And
             ( ( TPARTKBSTATUS.PartKBStatus = 'C' ) or 
               ( TPARTKBSTATUS.PartKBStatus is Null ) ) ) ) " update="TPARTKBSTATUS" updatewhere=1 updatekeyinplace=yes arguments=(("as_areaCode", string),("as_divCode", string),("as_suppCode", string),("as_itemCode", string)) )
