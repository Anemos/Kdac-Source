 retrieve="  SELECT distinct(~"DESLEO~".~"SLEDAB~".~"BITNO~" ||
         ~"DERCDO~".~"RCDDAC~".~"BCDNO~"),
         ~"DESLEO~".~"SLEDAB~".~"BCUSTCD~",   
         ~"DESLEO~".~"SLEDAB~".~"BITNO~" AS BTINO,   
         ~"DESLEO~".~"SLEDAB~".~"BITNO1~" AS BITNO1,   
         ~"DESLEO~".~"SLEDAB~".~"BITNM~",   
         ~"DESLEO~".~"SLEDAB~".~"BCARCD~",   
         ~"DERCDO~".~"RCDDAC~".~"BCDNO~" AS BCDNO,   
         ~"DEOPMO~".~"OPMDAB~".~"SDNAME~",   
         ~"DEOPMO~".~"OPMDAB~".~"SDUPCD~",   
         ~"DEOPMO~".~"OPMMAA~".~"SUPNM~",
         ~"DEOPMO~".~"OPMDAB~".~"SUPRC10~",   
         ~"DEOPMO~".~"OPMDAB~".~"SDPOUM~",   
         ~"DERCDO~".~"RCDDAC~".~"BUQTY~",   
         ~"DEOPMO~".~"OPMDAB~".~"SDCURR~"   
           
    FROM ~"DESLEO~".~"SLEDAB~",   
         ~"DERCDO~".~"RCDDAC~",   
         ~"DEOPMO~".~"OPMDAB~",   
         ~"DEOPMO~".~"OPMMAA~"  
   WHERE ( ~"DERCDO~".~"RCDDAC~".~"BPANO1~" = ~"DESLEO~".~"SLEDAB~".~"BITNO1~" ) and  
         ( ~"DERCDO~".~"RCDDAC~".~"BCDNO~" = ~"DEOPMO~".~"OPMDAB~".~"SVITNO~" ) and  
         ( ~"DEOPMO~".~"OPMMAA~".~"SUPCD~" = ~"DEOPMO~".~"OPMDAB~".~"SDUPCD~" ) and  
         ( ~"DESLEO~".~"SLEDAB~".~"BCUSTCD~" like 'L105' || '%'  ) 
ORDER BY 1 ASC,   
         2 ASC,   
         3 ASC,
         6 ASC   
" )
