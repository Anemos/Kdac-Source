 retrieve="  SELECT AS_AREACODE,   
         AS_DIVISIONCODE,   
         AS_STANDARDYEAR,   
         AS_LARGEGROUPCODE,   
         AS_LARGEGROUPNAME,   
		   AS_FSFLAG,
         AS_MONTH1,   
         AS_MONTH2,   
         AS_MONTH3,   
         AS_MONTH4,   
         AS_MONTH5,   
         AS_MONTH6,   
         AS_MONTH7,   
         AS_MONTH8,   
         AS_MONTH9,   
         AS_MONTH10,   
         AS_MONTH11,   
         AS_MONTH12,   
         AS_YEARGOAL  
    FROM (
		  SELECT A.AREACODE			AS AS_AREACODE,   
		         A.DIVISIONCODE		AS AS_DIVISIONCODE,   
		         B.STANDARDYEAR		AS AS_STANDARDYEAR,   
		         A.LARGEGROUPCODE	AS AS_LARGEGROUPCODE,   
		         A.LARGEGROUPNAME	AS AS_LARGEGROUPNAME,   
				   'F' 				AS AS_FSFLAG,
		         B.MONTH1			AS AS_MONTH1,   
		         B.MONTH2			AS AS_MONTH2,   
		         B.MONTH3			AS AS_MONTH3,   
		         B.MONTH4			AS AS_MONTH4,   
		         B.MONTH5			AS AS_MONTH5,   
		         B.MONTH6			AS AS_MONTH6,   
		         B.MONTH7			AS AS_MONTH7,   
		         B.MONTH8			AS AS_MONTH8,   
		         B.MONTH9			AS AS_MONTH9,   
		         B.MONTH10			AS AS_MONTH10,   
		         B.MONTH11			AS AS_MONTH11,   
		         B.MONTH12			AS AS_MONTH12,   
		         B.YEARGOAL			AS AS_YEARGOAL  
		    FROM TQLARGEGROUP				 A,   
		         TQLARGEGROUPTOGOALREJECT B  
		   WHERE A.AREACODE			*= B.AREACODE
		     AND A.DIVISIONCODE 	*= B.DIVISIONCODE
		     AND A.LARGEGROUPCODE	*= B.LARGEGROUPCODE
		     AND A.AREACODE			=  :as_areacode
		     AND A.DIVISIONCODE 	=  :as_divisioncode
		     AND B.STANDARDYEAR		=  :as_date
		
		UNION ALL
		
		  SELECT A.AREACODE			AS AS_AREACODE,   
		         A.DIVISIONCODE		AS AS_DIVISIONCODE,   
		         B.STANDARDYEAR		AS AS_STANDARDYEAR,   
		         A.LARGEGROUPCODE	AS AS_LARGEGROUPCODE,   
		         A.LARGEGROUPNAME	AS AS_LARGEGROUPNAME,   
				   'S' 				AS AS_FSFLAG,
		         B.MONTH1			AS AS_MONTH1,   
		         B.MONTH2			AS AS_MONTH2,   
		         B.MONTH3			AS AS_MONTH3,   
		         B.MONTH4			AS AS_MONTH4,   
		         B.MONTH5			AS AS_MONTH5,   
		         B.MONTH6			AS AS_MONTH6,   
		         B.MONTH7			AS AS_MONTH7,   
		         B.MONTH8			AS AS_MONTH8,   
		         B.MONTH9			AS AS_MONTH9,   
		         B.MONTH10			AS AS_MONTH10,   
		         B.MONTH11			AS AS_MONTH11,   
		         B.MONTH12			AS AS_MONTH12,   
		         B.YEARGOAL			AS AS_YEARGOAL  
		    FROM TQLARGEGROUP				 A,   
		         TQLARGEGROUPTOGOALREJECT B  
		   WHERE A.AREACODE			*= B.AREACODE
		     AND A.DIVISIONCODE 	*= B.DIVISIONCODE
		     AND A.LARGEGROUPCODE	*= B.LARGEGROUPCODE
		     AND A.AREACODE			=  :as_areacode
		     AND A.DIVISIONCODE 	=  :as_divisioncode
		     AND B.STANDARDYEAR		=  :as_date
         ) TMP
ORDER BY AS_AREACODE,   
         AS_DIVISIONCODE,   
         AS_STANDARDYEAR,   
         AS_LARGEGROUPCODE,   
		   AS_FSFLAG
 " arguments=(("as_areacode", string),("as_divisioncode", string),("as_date", string)) )
