global function string f_get_accdr047 (string a_manage1);int    l_n_rcnt

select count(*)
into   :l_n_rcnt
from   "PBCOMMON"."DAC002"
where  "PBCOMMON"."DAC002"."COMLTD"  = :g_s_company and
       "PBCOMMON"."DAC002"."COGUBUN" = 'ACC410'     and
		 "PBCOMMON"."DAC002"."COCODE"  = :a_manage1   using sqlca;
if isnull(l_n_rcnt) then
	l_n_rcnt = 0
end if
if l_n_rcnt = 0 then
	return 'e'
else
	return ''
end if
end function

