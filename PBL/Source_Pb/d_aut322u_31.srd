 retrieve="  SELECT A.COMLTD,   
         A.CHID,
			A.CHGDT,   
         A.ASID,   
         A.ASDT,   
         A.XPLANT,   
         A.DIV,   
         A.DEPT,   
         A.DEVICEGUBUN,   
         A.SPECGUBUN,   
         A.MODELNM,   
         A.MEMORY,   
         A.DISK,   
         A.CDRWEXIST,   
         A.MONITORSIZE,   
         A.MONITORGUBUN,   
         A.OS,   
         A.EMPGUBUN,   
         A.EMPNM,
			A.USBEXIST,   
         A.STCD,
			A.COMMENT,
			A.ETC,
			A.INPTID,   
         A.INPTDT,   
         A.UPDTID,   
         A.UPDTDT,   
         A.IPADDR,   
         A.MACADDR  
    FROM PBCOMMON.COMM810  AS A
   WHERE A.COMLTD = '01'  AND
			A.CHGDT BETWEEN :as_fchgdt AND :as_tchgdt AND
			A.XPLANT LIKE      :as_xplant      AND
			A.DIV    LIKE      :as_div         AND
			A.DEPT   LIKE      :as_dept        AND
			A.DEVICEGUBUN LIKE :as_devicegubun AND
			A.SPECGUBUN   LIKE :as_specgubun   AND
			A.EMPGUBUN    LIKE :as_empgubun    AND
			A.STCD LIKE        :as_stcd        AND
			A.CHID LIKE        :as_chid 
            
ORDER BY A.XPLANT, A.DIV, A.DEPT, A.CHID, A.CHGDT
" update="PBCOMMON.COMM810" updatewhere=0 updatekeyinplace=no arguments=(("as_fchgdt", string),("as_tchgdt", string),("as_xplant", string),("as_div", string),("as_dept", string),("as_devicegubun", string),("as_specgubun", string),("as_empgubun", string),("as_stcd", string),("as_chid", string)) )
