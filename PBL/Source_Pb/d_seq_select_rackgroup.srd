table(column=(type=char(3) updatewhereclause=yes name=productgroup dbname="TSEQRACKMST.PRODUCTGROUP" )
 column=(type=char(50) updatewhereclause=yes name=productname dbname="TSeqMstProd.PRODUCTNAME" )
 column=(type=char(3) updatewhereclause=yes name=rackgroup dbname="TSEQRACKMST.RACKGROUP" )
 column=(type=char(30) updatewhereclause=yes name=rackgroupname dbname="TSEQRACKMST.RackGroupName" )
 column=(type=char(35) updatewhereclause=yes name=displayname dbname="DisplayName" )
 retrieve="SELECT  TSEQRACKMST.PRODUCTGROUP ,
			TSEQMSTPROD.PRODUCTNAME ,
			TSEQRACKMST.RACKGROUP ,
			TSEQRACKMST.RackGroupName ,
			TSeqRackMst.RackGroupName + '(' + TSeqRackMst.RACKGROUP +')' as DisplayName    
        FROM TSEQRACKMST, TSEQMSTPROD      
        WHERE (TSEQRACKMST.FACTORYCODE LIKE :as_factorycode + '%')
				AND (TSEQRACKMST.PRODUCTGROUP = :as_productgroup)
				AND (TSEQRACKMST.RACKGROUP LIKE :as_rackgroup)
				AND (TSEQMSTPROD.AREACODE = :as_factorycode)
				AND (TSEQMSTPROD.DIVISIONCODE = :as_divisioncode)
				AND (TSEQMSTPROD.PRODUCTGROUP = :as_productgroup)
				AND (TSEQRACKMST.PRODUCTGROUP = TSEQMSTPROD.PRODUCTGROUP)   " arguments=(("as_factorycode", string),("as_divisioncode", string),("as_productgroup", string),("as_rackgroup", string)) )
