 retrieve="  SELECT TSeqMstItem.CustomerCode,   
         TSeqMstItem.CustomerItemCode,   
			nullif(TMstCustItem.CustItemName,''), 
			TMstDivision.DivisionName,  
			TSeqMstProd.ProductName,
         TSeqMstItem.AreaCode,   
         TSeqMstItem.DivisionCode,   
         TSeqMstItem.Itemcode,   
         TSeqMstItem.PartId,
         TSeqMstItem.PartId2,   
         TSeqMstItem.ProductGroup,  
			TSeqMstItem.seqgubun,
			TSeqMstItem.assygubun, 
         TSeqMstItem.LineCode,   
         TSeqMstItem.RackGroup,   
         TSeqMstItem.CarType,   
         TSeqMstItem.LotSize,   
         TSeqMstItem.SafeStockQty,   
         TSeqMstItem.LastEmp,   
         TSeqMstItem.LastDate  
    FROM TSeqMstItem  , TMstCustItem , TMstDivision , TSeqMstProd
   WHERE ( TSeqMstItem.AreaCode 				= :a_areacode ) AND  
         ( TSeqMstItem.DivisionCode 		like :a_divisioncode ) AND  
         ( TSeqMstItem.ProductGroup 		like :a_productgroup ) AND  
  			( TSeqMstItem.CustomerItemCode 	*=	TMstCustItem.CustItemCode ) And
			( TSeqMstItem.CustomerCode 	 	*=	TMstCustItem.CustCode ) And
			( TSeqMstItem.DivisionCode 		= 	TMstDivision.DivisionCode ) And
			( TSeqMstItem.AreaCode 				= 	TMstDivision.AreaCode ) And
			( TSeqMstItem.ProductGroup			=	TSeqMstProd.ProductGroup ) And
			( TSeqMstItem.DivisionCode 		= 	TSeqMstProd.DivisionCode ) And
			( TSeqMstItem.AreaCode 				= 	TSeqMstProd.AreaCode )" arguments=(("a_areacode", string),("a_divisioncode", string),("a_productgroup", string))  sort="areacode A divisioncode A productgroup A customercode A customeritemcode A itemcode A partid A " )
