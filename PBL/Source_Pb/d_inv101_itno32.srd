 retrieve="  SELECT ~"PBINV~".~"INV101~".~"XPLANT~",   
         ~"PBINV~".~"INV101~".~"DIV~",   
         ~"PBINV~".~"INV101~".~"ITNO~",   
         ~"PBINV~".~"INV002~".~"ITNM~",   
         ~"PBINV~".~"INV002~".~"SPEC~",   
         ~"PBINV~".~"INV002~".~"XUNIT~",   
         ~"PBINV~".~"INV002~".~"MAKER~",   
         ~"PBINV~".~"INV101~".~"CLS~",   
         ~"PBINV~".~"INV101~".~"SRCE~",   
         ~"PBINV~".~"INV101~".~"XPLAN~",   
         ~"PBINV~".~"INV101~".~"XUNIT~"  
    FROM ~"PBINV~".~"INV002~",   
         ~"PBINV~".~"INV101~"  
   WHERE ( ~"PBINV~".~"INV002~".~"COMLTD~" = ~"PBINV~".~"INV101~".~"COMLTD~" ) and  
         ( ~"PBINV~".~"INV002~".~"ITNO~" = ~"PBINV~".~"INV101~".~"ITNO~" ) and  
         ( ~"PBINV~".~"INV002~".~"COMLTD~" = '01' ) and 
         ( ~"PBINV~".~"INV101~".~"CLS~" = '22' ) AND  
         ( ~"PBINV~".~"INV002~".~"ITNM~" like :ag_itno )
" arguments=(("ag_itno", string))  sort="inv101_itno A " )
