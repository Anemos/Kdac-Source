 retrieve="  SELECT ~"PBSLE~".~"SLE501~".~"COMLTD~",   
         ~"PBSLE~".~"SLE501~".~"CSRNO~",   
         ~"PBSLE~".~"SLE501~".~"CSRNO1~",   
         ~"PBSLE~".~"SLE501~".~"CSRNO2~",   
         ~"PBSLE~".~"SLE501~".~"CITNO~",   
         ~"PBSLE~".~"SLE101~".~"CITNM~",   
         ~"PBSLE~".~"SLE501~".~"ITNO~",   
         ~"PBSLE~".~"SLE501~".~"ORDNO~",   
         ~"PBSLE~".~"SLE501~".~"DKDT~",   
         ~"PBSLE~".~"SLE501~".~"CNLCD~",   
         ~"PBSLE~".~"SLE501~".~"SUSE~",   
         ~"PBSLE~".~"SLE501~".~"CUR~",   
         ~"PBSLE~".~"SLE501~".~"LUPRC~",   
         ~"PBSLE~".~"SLE501~".~"DCQTY~",   
         ~"PBSLE~".~"SLE501~".~"STCD~",   
         ~"PBSLE~".~"SLE501~".~"SJCD~",
		   ~"PBSLE~".~"SLE501~".~"UPDTDT~",
			B.INPTDT
    FROM ~"PBSLE~".~"SLE101~",   
         ~"PBSLE~".~"SLE501~"
LEFT OUTER JOIN
(SELECT COMLTD,XPLANT,DIV,SRNO,INPTDT FROM PBINV.INV401
WHERE COMLTD = '01' AND
		SLIPTYPE = 'SR' AND
		XPLANT LIKE :arg2 AND
		DIV LIKE :arg3) B
ON B.COMLTD = ~"PBSLE~".~"SLE501~".~"COMLTD~" AND
	B.SRNO   =  ~"PBSLE~".~"SLE501~".~"CSRNO~"   
   WHERE ( ~"PBSLE~".~"SLE101~".~"COMLTD~" = '01' ) AND  
         ( ~"PBSLE~".~"SLE501~".~"DTYPE~" = 'C' ) AND  
         ( ~"PBSLE~".~"SLE501~".~"CUSTCD~" = :arg1 ) AND  
         ( ~"PBSLE~".~"SLE501~".~"XPLANT~" like :arg2 ) AND  
         ( ~"PBSLE~".~"SLE501~".~"DIV~" like :arg3 ) AND  
         ( ~"PBSLE~".~"SLE501~".~"DKDT~" between :arg4 AND :arg5 )  and    
         ( ~"PBSLE~".~"SLE101~".~"COMLTD~" = ~"PBSLE~".~"SLE501~".~"COMLTD~" ) and  
         ( ~"PBSLE~".~"SLE101~".~"CUSTCD~" = ~"PBSLE~".~"SLE501~".~"CUSTCD~" ) and  
         ( ~"PBSLE~".~"SLE101~".~"CITNO~" = ~"PBSLE~".~"SLE501~".~"CITNO~" )
" arguments=(("arg1", string),("arg2", string),("arg3", string),("arg4", string),("arg5", string)) )
